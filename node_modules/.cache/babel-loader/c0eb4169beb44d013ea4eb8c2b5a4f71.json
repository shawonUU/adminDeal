     });\n    },\n\n    getRatings(rating, maxRating = 5) {\n      return ratingGenerator(rating, maxRating);\n    },\n\n    addToWishList(id) {},\n\n    addToCompare(id) {},\n\n    showAddToCartModal() {}\n\n  }\n};","map":{"version":3,"mappings":"AAiTC,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAASC,eAAT,QAAgC,2BAAhC;AACA,eAAe;EACXC,KAAK,EAAE,CAAC,MAAD,CADI;;EAEXC,IAAI,GAAE;IACJ,OAAM;MACHC,oBAAoB,EAAC,EADlB;MAEHC,YAAY,EAAC,EAFV;MAGHC,YAAY,EAAC,YAHV;MAIHC,QAAQ,EAAE,CAJP;MAKHC,QAAQ,EAAE,GALP;MAOHC,OAAO,EAAE,IAPN;MAQHC,UAAS,EAAI,IARV;MASHC,iBAAiB,EAAE,EAThB;MAUHC,cAAc,EAAE,EAVb;MAWHC,QAAQ,EAAE,EAXP;MAYHC,MAAM,EAAE,EAZL;MAaHC,YAAY,EAAE,EAbX;MAcHC,gBAAgB,EAAE,EAdf;MAeHC,eAAe,EAAE,IAfd;MAgBHC,KAAK,EAAE,IAhBJ;MAiBHC,MAAM,EAAE,EAjBL;MAkBHC,UAAU,EAAE,IAlBT;MAmBHC,gBAAgB,EAAE,KAnBf;MAoBHC,uBAAuB,EAAE,IApBtB;MAqBHC,MAAM,EAAE,EArBL;MAsBHC,QAAQ,EAAC,EAtBN;MAuBHC,UAAU,EAAE,EAvBT;MAwBHC,WAAW,EAAC,CAxBT;MAyBHC,QAAQ,EAAC;IAzBN,CAAN;EA2BD,CA9BU;;EA+BXC,OAAO,GAAE;IACP;IACC,KAAKC,QAAL;EACF,CAlCU;;EAmCVC,OAAO,GAAE;IAEN,KAAKC,sBAAL,CAA4B,CAA5B;IACC,KAAKF,QAAL;EACH,CAvCS;;EAwCVG,KAAK,EAAC,EAxCI;EA2CVC,OAAO,EAAC;IACLJ,QAAQ,GAAE;MACP,IAAGK,QAAQ,CAACC,oBAAT,CAA8B,MAA9B,EAAsC,CAAtC,CAAH,EAA4C;QACxC,IAAIC,OAAM,GAAIC,MAAM,CAACC,QAAP,CAAgBC,MAA9B;QAEA,IAAIC,SAAQ,GAAIN,QAAQ,CAACO,aAAT,CAAuB,QAAvB,CAAhB;QACAD,SAAS,CAACE,GAAV,GAAgBN,OAAO,GAAC,uBAAxB;QACAF,QAAQ,CAACC,oBAAT,CAA8B,MAA9B,EAAsC,CAAtC,EAAyCQ,WAAzC,CAAqDH,SAArD;QAEA,IAAIA,SAAQ,GAAIN,QAAQ,CAACO,aAAT,CAAuB,QAAvB,CAAhB;QACAD,SAAS,CAACE,GAAV,GAAgBN,OAAO,GAAC,wBAAxB;QACAF,QAAQ,CAACC,oBAAT,CAA8B,MAA9B,EAAsC,CAAtC,EAAyCQ,WAAzC,CAAqDH,SAArD;MACJ;IACF,CAbI;;IAcLI,OAAO,GAAE;MACR,IAAIC,GAAE,GAAIC,CAAC,CAAC,+BAAD,CAAD,CAAmCC,IAAnC,EAAV;MACA,IAAIC,GAAE,GAAIF,CAAC,CAAC,gCAAD,CAAD,CAAoCC,IAApC,EAAV;;MACA,IAAG,KAAKvC,QAAL,IAAiBqC,GAAjB,IAAwB,KAAKtC,QAAL,IAAiByC,GAA5C,EAAgD;QAC7C,KAAKxC,QAAL,GAAgBqC,GAAhB;QACA,KAAKtC,QAAL,GAAgByC,GAAhB,CAF6C,CAG9C;MACF;IAEA,CAvBI;;IAwBJjB,sBAAsB,CAACkB,IAAD,EAAM;MACzB,KAAK3C,YAAL,GAAqB,YAArB;MACAN,KAAK,CAACkD,GAAN,CAAU,KAAKC,UAAL,GAA