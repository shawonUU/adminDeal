{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  data() {\n    return {\n      user: {\n        fullName: '',\n        email_or_phone: '',\n        password: '',\n        register_by: 'email',\n        confirmPassword: '',\n        agery: 0\n      }\n    };\n  },\n\n  methods: {\n    createUser() {\n      let token = document.head.querySelector(\"meta[name='csrf-token']\");\n\n      if (token) {\n        window.axios.defaults.headers.common['X-CSRF-TOKEN'] = token.content;\n      } else {\n        console.error('CSRF token not found');\n      }\n\n      axios.post(this.rootDomain + 'vue/v3/auth/signup', {\n        params: {\n          user: user\n        }\n      }).then(response => {\n        console.log(response);\n      });\n    },\n\n    toggleEmailPhone(ele) {\n      alert(ele);\n\n      if (this.register_by == 'email') {\n        this.register_by = 'phone';\n        ele.innerHTML = 'Use Phone Instead';\n      } else {\n        this.register_by = 'email';\n        ele.innerHTML = 'Use Email Instead';\n      }\n    }\n\n  },\n\n  mounted() {// if (document.getElementById('my-datatable')) return; // was already loaded\n    // var scriptTag = document.createElement(\"script\");\n    // scriptTag.src = \"https://cdn.datatables.net/v/dt/dt-1.10.16/sl-1.2.5/datatables.min.js\";\n    // scriptTag.id = \"my-datatable\";\n    // document.getElementsByTagName('head')[0].appendChild(scriptTag);\n  }\n\n};","map":{"version":3,"mappings":"AAyFA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,IAAI,GAAE;IACJ,OAAM;MACFC,IAAI,EAAE;QACJC,QAAQ,EAAE,EADN;QAEJC,cAAc,EAAC,EAFX;QAGJC,QAAQ,EAAE,EAHN;QAIJC,WAAW,EAAE,OAJT;QAKJC,eAAe,EAAE,EALb;QAMJC,KAAK,EAAE;MANH;IADJ,CAAN;EAUD,CAZY;;EAabC,OAAO,EAAC;IACNC,UAAU,GAAE;MACV,IAAIC,KAAI,GAAIC,QAAQ,CAACC,IAAT,CAAcC,aAAd,CAA4B,yBAA5B,CAAZ;;MAEA,IAAIH,KAAJ,EAAW;QACPI,MAAM,CAACf,KAAP,CAAagB,QAAb,CAAsBC,OAAtB,CAA8BC,MAA9B,CAAqC,cAArC,IAAuDP,KAAK,CAACQ,OAA7D;MACJ,CAFA,MAEO;QACHC,OAAO,CAACC,KAAR,CAAc,sBAAd;MACJ;;MACArB,KAAK,CAACsB,IAAN,C