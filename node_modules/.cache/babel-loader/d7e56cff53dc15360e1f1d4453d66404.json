{"ast":null,"code":"import Slider from '@vueform/slider';\nimport VueAwesomePaginate from \"vue-awesome-paginate\";\nimport \"vue-awesome-paginate/dist/style.css\";\nimport axios from 'axios';\nexport default {\n  components: {\n    Slider\n  },\n\n  data() {\n    return {\n      blogs: [],\n      value: [0, 100]\n    };\n  },\n\n  mounted() {\n    this.getBlogs(this.rootDomain);\n  },\n\n  methods: {\n    getBlogs(rootDomain) {\n      axios.get(rootDomain + 'vue/blog').then(response => {\n        this.blogs = response.data.data;\n      }).catch(error => {});\n    },\n\n    getSlug(slug) {\n      this.$router.push({\n        name: \"BlogDetails\",\n        params: {\n          slug: slug\n        }\n      });\n    },\n\n    getRange(value) {\n      console.log($(\"#getRange\").html());\n    }\n\n  },\n\n  myChangeEvent(val) {\n    console.log(val);\n  },\n\n  mySelectEvent({\n    id,\n    text\n  }) {\n    console.log({\n      id,\n      text\n    });\n  }\n\n};","map":{"version":3,"mappings":"AAiCA,OAAOA,MAAP,MAAmB,iBAAnB;AACA,OAAOC,kBAAP,MAA+B,sBAA/B;AACA,OAAO,qCAAP;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,UAAU,EAAE;IACRH;EADQ,CADC;;EAIZI,IAAI,GAAE;IACL,OAAM;MACJC,KAAK,EAAC,EADF;MAEJC,KAAK,EAAE,CAAC,CAAD,EAAG,GAAH;IAFH,CAAN;EAIA,CATW;;EAUZC,OAAO,GAAE;IACN,KAAKC,QAAL,CAAc,KAAKC,UAAnB;EACF,CAZW;;EAaZC,OAAO,EAAC;IACPF,QAAQ,CAACC,UAAD,EAAY;MAClBP,KAAK,CAACS,GAAN,CAAUF,UAAU,GAAC,UAArB,EACCG,IADD,CACOC,QAAD,IAAY;QACd,KAAKR,KAAL,GAAaQ,QAAQ,CAACT,IAAT,CAAcA,IAA3B;MACH,CAHD,EAICU,KAJD,CAIQC,KAAD,IAAS,CAEf,CAND;IAOD,CATM;;IAUPC,OAAO,CAACC,IAAD,EAAM;MACT,KAAKC,OAAL,CAAaC,IAAb,CAAkB;QAClBC,IAAI,EAAE,aADY;QAElBC,MAAM,EAAE;UACNJ,IAAI,EAAEA;QADA;MAFU,CAAlB;IAMH,CAjBM;;IAkBPK,QAAQ,CAAChB,KAAD,EAAO;MACZiB,OAAO,CAACC,GAAR,CAAYC,CAAC,CAAC,WAAD,CAAD,CAAeC,IAAf,EAAZ;IACH;;EApBO,CAbI;;EAmCZC,aAAa,CAACC,GAAD,EAAK;IACTL,OAAO,CAACC,GAAR,CAAYI,GAAZ;EACH,CArCM;;EAsCXC,aAAa,CAAC;IAACC,EAAD;IAAKC;EAAL,CAAD,EAAY;IACrBR,OAAO,CAACC,GAAR,CAAY;MAACM,EAAD;MAAKC;IAAL,CAAZ;EACJ;;AAxCW,CAAf","names":["Slider","VueAwesomePaginate","axios","components","data","blogs","value","mounted","getBlogs","rootDomain","methods","get","then","response","catch","error","getSlug","slug","$router","push","name","params","getRange","console","log","$","html","myChangeEvent","val","mySelectEvent","id","text"],"sourceRoot":"","sources":["F:\\adminDeal\\src\\components\\pages\\FrontEnd\\Blog.vue"],"sourcesContent":["<template>\n  <section class=\"pb-4\">\n  <div class=\"container\">\n    <div class=\"card-columns\">\n      <div v-for=\"(blog,index) in blogs\" :key=\"index\" class=\"card mb-3 overflow-hidden shadow-sm\">\n        <a style=\"cursor:pointer\" @click=\"getSlug(blog.slug)\" class=\"text-reset d-block\">\n          <img :src=\"blog.banner\"  :alt=\"blog.title\" class=\"img-fluid lazyload \">\n        </a>\n        <div class=\"p-4\">\n          <h2 class=\"fs-18 fw-600 mb-1\">\n            <a  style=\"cursor:pointer\" @click=\"getSlug(blog.slug)\" class=\"text-reset\">{{ blog.name }}</a>\n          </h2>\n          <div class=\"mb-2 opacity-50\">\n            <i>{{ blog.category }}</i>\n          </div>\n          <p class=\"opacity-70 mb-4\" v-html=\"blog.short_description\"></p>\n          <a  style=\"cursor:pointer\" @click=\"getSlug(blog.slug)\" class=\"btn btn-soft-primary\"> View More </a>\n        </div>\n      </div>\n    </div>\n    <div class=\"aiz-pagination aiz-pagination-center mt-4\">\n        <div>\n          <div id=\"getRange\" class=\"output\">Data: {{ value }}</div>\n            <Slider @click=\"getRange()\" v-model=\"value\"/>\n          <a>Range</a>\n        </div>\n        \n    </div>\n  </div>\n</section>\n</template>\n\n<script>\nimport Slider from '@vueform/slider';\nimport VueAwesomePaginate from \"vue-awesome-paginate\";\nimport \"vue-awesome-paginate/dist/style.css\";\nimport axios from 'axios';\nexport default {\n  components: {\n      Slider,\n    },\n   data(){\n    return{\n      blogs:[],\n      value: [0,100],\n    }\n   },\n   mounted(){\n      this.getBlogs(this.rootDomain);\n   },\n   methods:{\n    getBlogs(rootDomain){\n      axios.get(rootDomain+'vue/blog')\n      .then((response)=>{\n          this.blogs = response.data.data;\n      })\n      .catch((error)=>{\n\n      });\n    },\n    getSlug(slug){\n        this.$router.push({\n        name: \"BlogDetails\",\n        params: {\n          slug: slug\n        }\n      });\n    },\n    getRange(value){\n       console.log($(\"#getRange\").html());\n    }\n   },\n   myChangeEvent(val){\n            console.log(val);\n        },\n    mySelectEvent({id, text}){\n        console.log({id, text})\n    }\n}\n\n</script>\n\n<style src=\"@vueform/slider/themes/default.css\"></style>\n"]},"metadata":{},"sourceType":"module"}