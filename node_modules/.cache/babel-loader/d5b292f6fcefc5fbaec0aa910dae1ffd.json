{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  data() {\n    return {\n      user: {\n        fullName: '',\n        email_or_phone: '',\n        password: '',\n        register_by: 'phone',\n        buttonText: 'Use Email Instead',\n        confirmPassword: '',\n        agery: 0\n      }\n    };\n  },\n\n  methods: {\n    createUser() {\n      // let token = document.head.querySelector(\"meta[name='csrf-token']\");\n      // if (token) {\n      //     axios.defaults.headers.common['X-CSRF-TOKEN'] = token.content;\n      // } else {\n      //     console.error('CSRF token not found');\n      // }\n      axios.get(this.rootDomain + 'vue/v3/auth/signup', {\n        params: {\n          user: this.user\n        }\n      }).then(response => {\n        console.log(response);\n      });\n    },\n\n    toggleEmailPhone() {\n      if (this.user.register_by == 'email') {\n        this.user.register_by = 'phone';\n        this.user.buttonText = 'Use Email Instead';\n      } else {\n        this.user.register_by = 'email';\n        this.user.buttonText = 'Use Phone Instead';\n      }\n    }\n\n  },\n\n  mounted() {// if (document.getElementById('my-datatable')) return; // was already loaded\n    // var scriptTag = document.createElement(\"script\");\n    // scriptTag.src = \"https://cdn.datatables.net/v/dt/dt-1.10.16/sl-1.2.5/datatables.min.js\";\n    // scriptTag.id = \"my-datatable\";\n    // document.getElementsByTagName('head')[0].appendChild(scriptTag);\n  }\n\n};","map":{"version":3,"mappings":"AAyFA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,IAAI,GAAE;IACJ,OAAM;MACFC,IAAI,EAAE;QACJC,QAAQ,EAAE,EADN;QAEJC,cAAc,EAAC,EAFX;QAGJC,QAAQ,EAAE,EAHN;QAIJC,WAAW,EAAE,OAJT;QAKJC,UAAU,EAAE,mBALR;QAMJC,eAAe,EAAE,EANb;QAOJC,KAAK,EAAE;MAPH;IADJ,CAAN;EAWD,CAbY;;EAcbC,OAAO,EAAC;IACNC,UAAU,GAAE;MACV;MAEA;MACA;MACA;MACA;MACA;MACAX,KAAK,CAACY,GAAN,CAAU,KAAKC,UAAL,GAAgB,oBAA1B,EAAgD;QAACC,MAAM,EAAC;UAACZ,IAAI,EAAC,KAAKA;QAAX;MAAR,CAAhD,EACCa,IADD,CACOC,QAAD,IAAY;QACdC,OAAO,C