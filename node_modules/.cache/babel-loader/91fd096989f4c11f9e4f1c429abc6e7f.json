{"ast":null,"code":"import axios from 'axios';\nimport { ratingGenerator } from '@/HelpersFunction/Helpers';\nexport default {\n  props: ['slug'],\n\n  data() {\n    return {\n      shopWiseProduct: [],\n      shopDetails: [],\n      ShowNotFound: 'Loading...',\n      currentPage: 1,\n      totalItems: \"\"\n    };\n  },\n\n  mounted() {\n    // console.log(this.slug);\n    this.getShopWiseProduct(1);\n  },\n\n  methods: {\n    getShopWiseProduct(page) {\n      this.ShowNotFound = 'Loading...';\n      axios.get(this.rootDomain + 'vue/v3/products/seller/' + this.slug + '?page=' + page).then(response => {\n        console.log(response.data[3]);\n        this.shopWiseProduct = response.data[0];\n        this.shopDetails = response.data[1];\n        this.totalItems = response.data[2];\n\n        if (response.data[0] < 1) {\n          this.ShowNotFound = 'Product Not Found!';\n        } else {\n          this.ShowNotFound = '';\n        }\n\n        this.scrollToTop();\n      });\n    },\n\n    getRatings(rating, maxRating = 5) {\n      return ratingGenerator(rating, maxRating);\n    },\n\n    productDetails(slug) {\n      this.$router.push({\n        name: \"singleProduct\",\n        params: {\n          slug: slug\n        }\n      });\n    },\n\n    scrollToTop() {\n      window.scrollTo(0, 400);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAoKA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAASC,eAAT,QAAgC,2BAAhC;AACA,eAAc;EACVC,KAAK,EAAC,CAAC,MAAD,CADI;;EAEVC,IAAI,GAAE;IACF,OAAM;MACFC,eAAe,EAAC,EADd;MAEFC,WAAW,EAAC,EAFV;MAGFC,YAAY,EAAC,YAHX;MAIFC,WAAW,EAAC,CAJV;MAKFC,UAAU,EAAC;IALT,CAAN;EAOH,CAVS;;EAWVC,OAAO,GAAE;IACL;IACA,KAAKC,kBAAL,CAAwB,CAAxB;EACH,CAdS;;EAeVC,OAAO,EAAC;IACJD,kBAAkB,CAACE,IAAD,EAAM;MACnB,KAAKN,YAAL,GAAqB,YAArB;MACDN,KAAK,CAACa,GAAN,CAAU,KAAKC,UAAL,GAAgB,yBAAhB,GAA0C,KAAKC,IAA/C,GAAoD,QAApD,GAA6DH,IAAvE,EACCI,IADD,CACOC,QAAD,IAAY;QACfC,OAAO,CAACC,GAAR,CAAYF,QAAQ,CAACd,IAAT,CAAc,CAAd,CAAZ;QACG,KAAKC,eAAL,GAAuBa,QAAQ,CAACd,IAAT,CAAc,CAAd,CAAvB;QACA,KAAKE,WAAL,G