{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      auth: {\n        isAuthenticated: false,\n        user: {}\n      },\n      wishlist: {}\n    };\n  },\n\n  created() {\n    var user = localStorage.getItem(\"user\");\n\n    if (user !== null) {\n      user = JSON.parse(user);\n      this.auth.isAuthenticated = true;\n      this.auth.user = user;\n      this.getWishList();\n    }\n  },\n\n  methods: {\n    getWishList() {\n      // console.log(\"############################\");\n      axios.get(this.selfDomain + 'vueweb/wishlists', {\n        // axios.get(this.selfDomain+'vue/v3/wishlists', {\n        headers: {\n          Authorization: \"Bearer \" + this.auth.user.access_token\n        }\n      }).then(res => {\n        this.wishlist = res.data.wishlists.data; // console.log(res.data);\n        //     console.log(\"ComeEEEEEE\");\n      }).catch(err => {\n        console.log(err);\n      });\n    },\n\n    productDetails(slug, product) {\n      this.$router.push({\n        name: \"singleProduct\",\n        params: {\n          slug: slug\n        }\n      });\n      let recentViewProduct = {};\n\n      if (this.$cookies.get('recentViewProducts') !== null) {\n        recentViewProduct = this.$cookies.get('recentViewProducts');\n      }\n\n      recentViewProduct[product.id] = product; // console.log(product.id);\n      // console.log(recentViewProduct);\n\n      this.$cookies.set('recentViewProducts', recentViewProduct);\n    },\n\n    digitalProductDetails(slug, product) {\n      this.$router.push({\n        name: \"DigitalProductDetails\",\n        params: {\n          slug: slug\n        }\n      });\n      let recentViewProduct = {};\n\n      if (this.$cookies.get('recentViewProducts') !== null) {\n        recentViewProduct = this.$cookies.get('recentViewProducts');\n      }\n\n      recentViewProduct[product.id] = product;\n      this.$cookies.set('recentViewProducts', recentViewProduct);\n    },\n\n    removeWishlistItem(id, index) {\n      axios.get(this.selfDomain + 'vueweb/wishlists_remove', {\n        params: {\n          id: id\n        },\n        headers: {\n          Authorization: \"Bearer \" + this.auth.user.access_token\n        }\n      }).then(response => {\n        this.wishlist.splice(index, 1);\n        this.emitter.emit(\"reload\", true);\n      }).catch(err => {\n        console.log(err);\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAoEG,OAAOA,KAAP,MAAkB,OAAlB;AACC,eAAe;EAGXC,IAAI,GAAE;IACF,OAAM;MACFC,IAAI,EAAC;QACDC,eAAe,EAAE,KADhB;QAEDC,IAAI,EAAE;MAFL,CADH;MAKFC,QAAQ,EAAC;IALP,CAAN;EAOH,CAXU;;EAYXC,OAAO,GAAE;IACN,IAAIF,IAAG,GAAIG,YAAY,CAACC,OAAb,CAAqB,MAArB,CAAX;;IACC,IAAGJ,IAAG,KAAM,IAAZ,EAAiB;MACbA,IAAG,GAAIK,IAAI,CAACC,KAAL,CAAWN,IAAX,CAAP;MACA,KAAKF,IAAL,CAAUC,eAAV,GAA4B,IAA5B;MACA,KAAKD,IAAL,CAAUE,IAAV,GAAiBA,IAAjB;MACA,KAAKO,WAAL;IACJ;EACH,CApBU;;EAqBXC,OAAO,EAAC;IAEJD,WAAW,GAAE;MACT;MACAX,KAAK,CAACa,GAAN,CAAU,KAAKC,UAAL,GAAgB,kBAA1B,EAA8C;QAC1C;QACAC,OAAO,EAAE;UACLC,aAAa,EAAE,YAAY,KAAKd,IAAL,CAAUE,IAAV,CAAea;QADrC;MAFiC,CAA9C,EAKGC,IALH,CAKQC,GAAG,IAAE;QAEb,KAAKd,QAAL,GAAgBc,GAAG,CAAClB,IAAJ,CAASmB,SAAT,CAAmBnB,IAAnC,CAFa,CAGb;QACA;MACC,CAVD,EAUGoB,KAVH,CAUSC,GAAG,IAAE;QACVC,OAAO,CAACC,GAAR,CAAYF,GAAZ;MACH,CAZD;IAaH,CAjBG;;IAmBJG,cAAc,CAACC,IAAD,EAAMC,OAAN,EAAc;MACxB,KAAKC,OAAL,CAAaC,IAAb,CAAkB;QACdC,IAAI,EAAE,eADQ;QAEdC,MAAM,EAAE;UACRL,IAAI,EAAEA;QADE;MAFM,CAAlB;MAQA,IAAIM,iBAAiB,GAAC,EAAtB;;MACA,IAAG,KAAKC,QAAL,CAAcpB,GAAd,CAAkB,oBAAlB,MAA4C,IAA/C,EAAoD;QAChDmB,iBAAgB,GAAI,KAAKC,QAAL,CAAcpB,GAAd,CAAkB,oBAAlB,CAApB;MACJ;;MACAmB,iBAAiB,CAACL,OAAO,CAACO,EAAT,CAAjB,GAAgCP,OAAhC,CAbwB,CAcxB;MACA;;MACA,KAAKM,QAAL,CAAcE,GAAd,CAAkB,oBAAlB,EAAuCH,iBAAvC;IAEH,CArCG;;IAsCJI,qBAAqB,CAACV,IAAD,EAAMC,OAAN,EAAc;MAC/B,KAAKC,OAAL,CAAaC,IAAb,CAAkB;QACdC,IAAI,EAAE,uBADQ;QAEdC,MAAM,EAAE;UACRL,IAAI,EAAEA;QADE;MAFM,CAAlB;MAOA,IAAIM,iBAAiB,GAAC,EAAtB;;MACA,IAAG,KAAKC,QAAL,CAAcpB,GAAd,CAAkB,oBAAlB,MAA4C,IAA/C,EAAoD;QAChDmB,iBAAgB,GAAI,KAAKC,QAAL,CAAcp