{"ast":null,"code":"// Import Swiper Vue.js components\nimport { Swiper, SwiperSlide } from \"swiper/vue\";\nimport { Autoplay, Pagination, Navigation } from \"swiper\"; // Import Swiper styles\n\nimport \"swiper/css\";\nimport \"swiper/css/pagination\";\nimport \"swiper/css/navigation\";\nimport TodaysOffer from \"./ProductComponents/TodaysOffer.vue\";\nimport { useCategoryStore } from \"@/Store/Categories\";\nimport { mapState, mapActions } from \"pinia\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      modules: [Autoplay, Pagination, Navigation]\n    };\n  },\n\n  components: {\n    TodaysOffer,\n    Swiper,\n    SwiperSlide\n  },\n  computed: { ...mapState(useCategoryStore, ['categories']),\n    ...mapState(useCategoryStore, ['subcategories']),\n    ...mapState(useCategoryStore, ['featuredCategories'])\n  },\n\n  mounted() {\n    this.getCat(this.rootDomain); // console.log(this.rootDomain);\n  },\n\n  methods: { ...mapActions(useCategoryStore, ['getCat']),\n    ...mapActions(useCategoryStore, ['getSubcategory'])\n  }\n};","map":{"version":3,"mappings":"AAmFA;AACA,SAASA,MAAT,EAAiBC,WAAjB,QAAoC,YAApC;AACA,SAASC,QAAT,EAAmBC,UAAnB,EAA+BC,UAA/B,QAAiD,QAAjD,C,CACA;;AACA,OAAO,YAAP;AACA,OAAO,uBAAP;AACA,OAAO,uBAAP;AACA,OAAOC,WAAP,MAAwB,qCAAxB;AACA,SAASC,gBAAT,QAAiC,oBAAjC;AACA,SAASC,QAAT,EAAmBC,UAAnB,QAAoC,OAApC;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,IAAI,GAAE;IACJ,OAAM;MACJC,OAAO,EAAE,CAACT,QAAD,EAAWC,UAAX,EAAuBC,UAAvB;IADL,CAAN;EAGD,CALY;;EAMdQ,UAAU,EAAC;IAACP,WAAD;IAAcL,MAAd;IAAqBC;EAArB,CANG;EAQdY,QAAQ,EAAC,EACR,GAAGN,QAAQ,CAACD,gBAAD,EAAkB,CAAC,YAAD,CAAlB,CADH;IAER,GAAGC,QAAQ,CAACD,gBAAD,EAAkB,CAAC,eAAD,CAAlB,CAFH;IAGR,GAAGC,QAAQ,CAACD,gBAAD,EAAkB,CAAC,oBAAD,CAAlB;EAHH,CARK;;EAcdQ,OAAO,GAAE;IACR,KAAKC,MAAL,CAAY,KAAKC,UAAjB,EADQ,CAER;EACA,CAjBa;;EAmBdC,OAAO,EAAC,EACP,GAAGT,UAAU,CAACF,gBAAD,EAAkB,CAAC,QAAD,CAAlB,CADN;IAEP,GAAGE,UAAU,CAACF,gBAAD,EAAkB,CAAC,gBAAD,CAAlB;EAFN;AAnBM,CAAf","names":["Swiper","SwiperSlide","Autoplay","Pagination","Navigation","TodaysOffer","useCategoryStore","mapState","mapActions","axios","data","modules","components","computed","mounted","getCat","rootDomain","methods"],"sourceRoot":"","sources":["F:\\adminDeal\\src\\components\\layouts\\HomeBanner.vue"],"sourcesContent":["<template>\n    <div class=\"home-banner-area mb-4 pt-3\">\n   <div class=\"container\">\n     <div class=\"row gutters-10 position-relative\">\n       <div class=\"col-lg-3 position-static d-none d-lg-block\">\n         <div class=\"aiz-category-menu bg-white rounded  shadow-sm\">\n           <ul class=\"list-unstyled categories no-scrollbar py-2 mb-0 text-left\">\n             <li v-for=\"(category,index) in categories\" :key=\"index\" class=\"category-nav-element\" data-id=\"{{ category.id }}\">\n               <a  href=\"https://admindeal.com.bd/category/man-woman-baby-fashion\" class=\"text-truncate text-reset py-2 px-3 d-block\">\n                 <img class=\"cat-image lazyload mr-2 opacity-60\" src=\"https://admindeal.com.bd/public/assets/img/placeholder.jpg\" data-src=\"https://admindeal.s3.ap-southeast-1.amazonaws.com/uploads/all/k9PEQwECIZVCZAujoWuvhWvLIdEhlPhFdsGf4HNT.png\" width=\"16\" alt=\"Men&#039;s, Women&#039;s &amp; Baby&#039;s fashion\" onerror=\"this.onerror=null;this.src='https://admindeal.com.bd/public/assets/img/placeholder.jpg';\">\n                 <span @mouseover=\"getSubcategory(this.rootDomain,category.id)\" class=\"cat-name\">{{ category.name }}</span>\n               </a>\n               <div class=\"sub-cat-menu c-scrollbar-light rounded shadow-lg p-4\">\n                  <div v-if=\"subcategories.length==0\" class=\" text-center absolute-center\">\n                    <i class=\"las la-spinner la-spin la-3x opacity-70\"></i>\n                  </div>\n                  <div v-else class=\"card-columns\">\n                        <div v-for=\"(subcategory, index) in subcategories\" :key=\"index\" class=\"card shadow-none border-0\">\n                            <ul class=\"list-unstyled mb-3\">\n                                <li class=\"fw-600 border-bottom pb-2 mb-3\">\n                                    <a class=\"text-reset\" href=\"\">{{ subcategory.name }}</a>\n                                </li>\n                                  <li v-for=\"(childcategory, index) in subcategory.childCategorys\" :key=\"index\" class=\"mb-2\">\n                                      <a class=\"text-reset\" href=\"\">{{ childcategory.name }}</a>\n                                  </li>\n                            </ul>\n                        </div>\n                </div>\n               </div>\n             </li>\n           </ul>\n         </div>\n       </div>\n       <div class=\" col-lg-9 \">\n         <div class=\"aiz-carousel dots-inside-bottom mobile-img-auto-height\" data-arrows=\"true\" data-dots=\"true\" data-autoplay=\"true\">\n            <swiper :spaceBetween=\"30\"\n                    :centeredSlides=\"true\"\n                    :autoplay=\"{\n                      delay: 3000,\n                      disableOnInteraction: false,\n                    }\"\n                    :pagination=\"{\n                      clickable: true,\n                    }\"\n                    :navigation=\"true\"\n                    :modules=\"modules\"\n                    class=\"mySwiper\">\n                  <swiper-slide  class=\"\">\n                   <div class=\"carousel-box\">\n                    <a href=\"#\">\n                       <img class=\"d-block mw-100 img-fit rounded shadow-sm overflow-hidden\" src=\"https://admindeal.s3.ap-southeast-1.amazonaws.com/uploads/all/b4ZCemioim6ddcsoQsgv6XsUmN2gQ10uNlTsUECR.webp\" alt=\"Admin Deal promo\" height=\"320\">\n                    </a>\n                  </div>\n                </swiper-slide>\n                <swiper-slide  class=\"\">\n                   <div class=\"carousel-box\">\n                    <a href=\"#\">\n                       <img class=\"d-block mw-100 img-fit rounded shadow-sm overflow-hidden\" src=\"https://admindeal.s3.ap-southeast-1.amazonaws.com/uploads/all/b4ZCemioim6ddcsoQsgv6XsUmN2gQ10uNlTsUECR.webp\" alt=\"Admin Deal promo\" height=\"320\">\n                    </a>\n                  </div>\n                </swiper-slide>\n              \n            </swiper>\n         </div>\n         <ul class=\"list-unstyled mb-0 row gutters-5\">\n           <li v-for=\"(featuredCategory, index) in featuredCategories\" :key=\"index\" class=\"minw-0 col-4 col-md mt-3\">\n              <router-link :to=\"{name:'blog'}\" class=\"d-block rounded bg-white p-2 text-reset shadow-sm\">\n                <img :src=\"featuredCategory.banner\" alt=\"Groceries, Lifestyle &amp; Medical\" class=\"lazyload img-fit\" height=\"78\">\n                <div class=\"text-truncate fs-12 fw-600 mt-2 opacity-70\">{{ featuredCategory.name }}</div>\n              </router-link>\n            </li>\n          \n         </ul>\n       </div>\n       <!---->\n     </div>\n   </div>\n </div>\n    <TodaysOffer></TodaysOffer>\n    \n</template>\n\n<script>\n// Import Swiper Vue.js components\nimport { Swiper, SwiperSlide } from \"swiper/vue\";\nimport { Autoplay, Pagination, Navigation } from \"swiper\";\n// Import Swiper styles\nimport \"swiper/css\";\nimport \"swiper/css/pagination\";\nimport \"swiper/css/navigation\";\nimport TodaysOffer from \"./ProductComponents/TodaysOffer.vue\";\nimport { useCategoryStore } from \"@/Store/Categories\";\nimport { mapState, mapActions} from \"pinia\";\nimport axios from \"axios\";\nexport default {\n  data(){\n    return{\n      modules: [Autoplay, Pagination, Navigation],\n    }\n  },\n components:{TodaysOffer, Swiper,SwiperSlide,},\n \n computed:{\n  ...mapState(useCategoryStore,['categories']),\n  ...mapState(useCategoryStore,['subcategories']),\n  ...mapState(useCategoryStore,['featuredCategories']),\n },\n\n mounted(){\n  this.getCat(this.rootDomain);\n  // console.log(this.rootDomain);\n },\n\n methods:{\n  ...mapActions(useCategoryStore,['getCat']),\n  ...mapActions(useCategoryStore,['getSubcategory']),\n }\n\n}\n</script>\n\n<style>\n.swiper-button-next:after, .swiper-button-prev:after {\n    background: #2eb2ff00!important;\n    font-family: swiper-icons;\n    font-size: 20px!important;\n    text-transform: none!important;\n    letter-spacing: 0;\n    font-variant: initial;\n    line-height: 1;\n    color: #000!important;\n}\n</style>"]},"metadata":{},"sourceType":"module"}