{"ast":null,"code":"import { Swiper, SwiperSlide } from \"swiper/vue\";\nimport { ratingGenerator } from '@/HelpersFunction/Helpers'; // Import Swiper styles\n\nimport \"swiper/css\";\nimport \"swiper/css/free-mode\";\nimport \"swiper/css/pagination\";\nimport { FreeMode, Navigation } from \"swiper\";\nimport { useProductStore } from \"@/Store/Product\";\nimport { mapState, mapActions } from \"pinia\";\nimport axios from \"axios\";\nexport default {\n  props: ['relatedProducts'],\n\n  data() {\n    return {\n      modules: [FreeMode, Navigation]\n    };\n  },\n\n  components: {\n    Swiper,\n    SwiperSlide\n  },\n\n  mounted() {\n    console.log(this.relatedProducts);\n  },\n\n  methods: {\n    productDetails(slug) {\n      this.$router.push({\n        name: \"singleProduct\",\n        params: {\n          slug: slug\n        }\n      });\n    },\n\n    digitalProductDetails(slug) {\n      this.$router.push({\n        name: \"DigitalProductDetails\",\n        params: {\n          slug: slug\n        }\n      });\n    },\n\n    getRatings(rating, maxRating = 5) {\n      return ratingGenerator(rating, maxRating);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAyFE,SAASA,MAAT,EAAiBC,WAAjB,QAAoC,YAApC;AACA,SAASC,eAAT,QAAgC,2BAAhC,C,CACA;;AACA,OAAO,YAAP;AACA,OAAO,sBAAP;AACA,OAAO,uBAAP;AACA,SAASC,QAAT,EAAkBC,UAAlB,QAAoC,QAApC;AACA,SAASC,eAAT,QAAgC,iBAAhC;AACA,SAASC,QAAT,EAAmBC,UAAnB,QAAoC,OAApC;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,KAAK,EAAC,CAAC,iBAAD,CADO;;EAEbC,IAAI,GAAE;IACJ,OAAM;MACJC,OAAO,EAAE,CAACR,QAAD,EAAUC,UAAV;IADL,CAAN;EAID,CAPY;;EAQbQ,UAAU,EAAE;IACVZ,MADU;IAEVC;EAFU,CARC;;EAYbY,OAAO,GAAE;IACLC,OAAO,CAACC,GAAR,CAAY,KAAKC,eAAjB;EACH,CAdY;;EAebC,OAAO,EAAE;IACPC,cAAc,CAACC,IAAD,EAAM;MAClB,KAAKC,OAAL,CAAaC,IAAb,CAAkB;QAChBC,IAAI,EAAE,eADU;QAEhBC,MAAM,EAAE;UACNJ,IAAI,EAAEA;QADA;MAFQ,CAAlB;IAMD,CARM;;IASPK,qBAAqB,CAACL,IAAD,EAAM;MACzB,KAAKC,OAAL,CAAaC,IAAb,CAAkB;QAChBC,IAAI,EAAE,uBADU;QAEhBC,MAAM,EAAE;UACNJ,IAAI,EAAEA;QADA;MAFQ,CAAlB;IAMD,CAhBM;;IAiBPM,UA