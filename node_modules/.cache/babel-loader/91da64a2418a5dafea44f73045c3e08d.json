mg src=\"https://admindeal.com.bd/public/assets/img/placeholder.jpg\" :data-src=\"product.thumbnail_image\" class=\"img-fit lazyload mx-auto h-140px h-md-210px\"  alt=\"Black Plated Finger Ring For Mens\" >\n               </a>\n               <div class=\"absolute-top-right aiz-p-hov-icon\">\n                 <a href=\"javascript:void(0)\" onclick=\"addToWishList(5931)\" data-toggle=\"tooltip\" data-title=\"Add to wishlist\" data-placement=\"left\">\n                   <i class=\"la la-heart-o\"></i>\n                 </a>\n                 <a href=\"javascript:void(0)\" onclick=\"addToCompare(5931)\" data-toggle=\"tooltip\" data-title=\"Add to compare\" data-placement=\"left\">\n                   <i class=\"las la-sync\"></i>\n                 </a>\n                 <a href=\"javascript:void(0)\" onclick=\"showAddToCartModal(5931)\" data-toggle=\"tooltip\" data-title=\"Add to cart\" data-placement=\"left\">\n                   <i class=\"las la-shopping-cart\"></i>\n                 </a>\n               </div>\n             </div>\n             <div class=\"p-md-3 p-2 text-left\">\n               <div class=\"fs-15\">\n                 <del v-if=\"product.stroked_price\" class=\"fw-600 opacity-50 mr-1\">{{ product.stroked_price }}</del>\n                 <span  v-if=\"product.main_price\" class=\"fw-700 text-primary\">{{ product.main_price }} <span class=\"my-danger\" style=\"color: #000 !important; font-size: 12px;\">&nbsp;{{ product.discount }}</span>\n                 </span>\n                 <del v-if=\"product.base_price\" class=\"fw-600 opacity-50 mr-1\">৳{{ product.base_price }}</del>\n                   <span v-if=\"product.base_discounted_price\" class=\"fw-700 text-primary\">৳{{ product.base_discounted_price }} <span class=\"my-danger\" style=\"color: #000 !important; font-size: 12px;\">&nbsp;-৳{{ product.discount }}</span>\n                   </span>\n               </div>\n               <div class=\"rating rating-sm mt-1\">\n                      <template v-for=\"index in 5\" :key=\"index\">\n                          <i v-if=\"index<=product.rating\" class = 'las la-star active'></i>\n                          <i v-else class = 'las la-star'></i>\n                      </template>\n                      ({{ product.rating }})\n                </div>\n               <h3 class=\"fw-600 fs-13 text-truncate-2 lh-1-4 mb-0 h-35px\">\n                <a style=\"cursor:pointer\"  @click=\"product.digital==0?productDetails(product.slug):digitalProductDetails(product.slug)\"  :to=\"{name:'singleProduct'}\" class=\"d-block text-reset\">{{ product.name}}</a>\n               </h3>\n               <!-- <div class=\"rounded px-2 mt-2 bg-soft-primary border-soft-primary border\"> Cashback : <span class=\"fw-700 float-right\">0</span>\n               </div> -->\n             </div>\n           </div>\n         </div> \n        </swiper-slide>\n         \n      </swiper>\n</template>\n\n<script>\nimport { Swiper, SwiperSlide } from \"swiper/vue\";\nimport { ratingGenerator } from '@/HelpersFunction/Helpers';\n// Import Swiper styles\nimport \"swiper/css\";\nimport \"swiper/css/free-mode\";\nimport \"swiper/css/pagination\";\nimport { FreeMode,Navigation } from \"swiper\";\nimport axios from \"axios\";\nexport default {\n    props:['products'],\n    data(){\n    return{\n     auth:{\n            isAuthenticated: false,\n            user: {},\n        },\n      modules: [FreeMode,Navigation],\n    }\n  },\n  components: {\n    Swiper,\n    SwiperSlide,\n  },\n  created(){\n        var user = localStorage.getItem(\"user\");\n        if(user !== null){\n            user = JSON.parse(user);\n            this.auth.isAuthenticated = true;\n            this.auth.user = user;\n        }\n        \n    },\n  mounted(){\n    console.log(this.$cookies.get('recentViewProducts'))\n  },\n  methods:{\n    productDetails(slug,product){\n      this.$router.push({\n        name: \"singleProduct\",\n        params: {\n          slug: slug\n        }\n      });\n      console.log(product.id);\n     \n     let recentViewProduct=[]\n     if(this.$cookies.get('recentViewProducts') !== null){\n        // recentViewProduct = this.$cookies.get('recentViewProducts')\n        this.$cookies.get('recentViewProducts')\n     }\n      recentViewProduct.push(product);\n      let products =Object.setPrototypeOf(recentViewProduct, Object.prototype);\n      this.$cookies.set('recentViewProducts',products);\n\n    },\n    digitalProductDetails(slug,product){\n      this.$router.push({\n        name: \"DigitalProductDetails\",\n        params: {\n          slug: slug\n        }\n      });\n      let recentViewProduct=[]\n     if(this.$cookies.get('recentViewProducts') !== null){\n        recentViewProduct = this.$cookies.get('recentViewProducts')\n      }\n      console.log(product.id);\n      recentViewProduct.push(product);\n      let products =Object.setPrototypeOf(recentViewProduct, Object.prototype);\n      this.$cookies.set('recentViewProducts',products);\n\n    },\n    getRatings(rating,maxRating=5){\n           return ratingGenerator(rating,maxRating)\n        },\n  }\n}\n</script>\n\n<style>\n\n</style>"]},"metadata":{},"sourceType":"module"}