{"ast":null,"code":"import { Swiper, SwiperSlide } from \"swiper/vue\";\nimport { ratingGenerator } from '@/HelpersFunction/Helpers'; // Import Swiper styles\n\nimport \"swiper/css\";\nimport \"swiper/css/free-mode\";\nimport \"swiper/css/pagination\";\nimport { FreeMode, Navigation } from \"swiper\";\nimport axios from \"axios\";\nexport default {\n  props: ['products'],\n\n  data() {\n    return {\n      auth: {\n        isAuthenticated: false,\n        user: {}\n      },\n      modules: [FreeMode, Navigation]\n    };\n  },\n\n  components: {\n    Swiper,\n    SwiperSlide\n  },\n\n  created() {\n    var user = localStorage.getItem(\"user\");\n\n    if (user !== null) {\n      user = JSON.parse(user);\n      this.auth.isAuthenticated = true;\n      this.auth.user = user;\n    }\n  },\n\n  mounted() {\n    console.log(this.$cookies.get('recentViewProducts'));\n  },\n\n  methods: {\n    productDetails(slug, product) {\n      this.$router.push({\n        name: \"singleProduct\",\n        params: {\n          slug: slug\n        }\n      });\n      console.log(product.id);\n      let recentViewProduct = [];\n\n      if (this.$cookies.get('recentViewProducts') !== null) {\n        // recentViewProduct = this.$cookies.get('recentViewProducts')\n        this.$cookies.get('recentViewProducts');\n      }\n\n      recentViewProduct.push(product);\n      let products = Object.setPrototypeOf(recentViewProduct, Object.prototype);\n      this.$cookies.set('recentViewProducts', products);\n    },\n\n    digitalProductDetails(slug, product) {\n      this.$router.push({\n        name: \"DigitalProductDetails\",\n        params: {\n          slug: slug\n        }\n      });\n      let recentViewProduct = [];\n\n      if (this.$cookies.get('recentViewProducts') !== null) {\n        recentViewProduct = this.$cookies.get('recentViewProducts');\n      }\n\n      console.log(product.id);\n      recentViewProduct.push(product);\n      let products = Object.setPrototypeOf(recentViewProduct, Object.prototype);\n      this.$cookies.set('recentViewProducts', products);\n    },\n\n    getRatings(rating, maxRating = 5) {\n      return ratingGenerator(rating, maxRating);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAyEA,SAASA,MAAT,EAAiBC,WAAjB,QAAoC,YAApC;AACA,SAASC,eAAT,QAAgC,2BAAhC,C,CACA;;AACA,OAAO,YAAP;AACA,OAAO,sBAAP;AACA,OAAO,uBAAP;AACA,SAASC,QAAT,EAAkBC,UAAlB,QAAoC,QAApC;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;EACXC,KAAK,EAAC,CAAC,UAAD,CADK;;EAEXC,IAAI,GAAE;IACN,OAAM;MACLC,IAAI,EAAC;QACEC,eAAe,EAAE,KADnB;QAEEC,IAAI,EAAE;MAFR,CADA;MAKJC,OAAO,EAAE,CAACR,QAAD,EAAUC,UAAV;IALL,CAAN;EAOD,CAVY;;EAWbQ,UAAU,EAAE;IACVZ,MADU;IAEVC;EAFU,CAXC;;EAebY,OAAO,GAAE;IACH,IAAIH,IAAG,GAAII,YAAY,CAACC,OAAb,CAAqB,MAArB,CAAX;;IACA,IAAGL,IAAG,KAAM,IAAZ,EAAiB;MACbA,IAAG,GAAIM,IAAI,CAACC,KAAL,CAAWP,IAAX,CAAP;MACA,KAAKF,IAAL,CAAUC,eAAV,GAA4B,IAA5B;MACA,KAAKD,IAAL,CAAUE,IAAV,GAAiBA,IAAjB;IACJ;EAEH,CAvBU;;EAwBbQ,OAAO,GAAE;IACPC,OAAO,CAACC,GAAR,CAAY,KAAKC,QAAL,CAAcC,GAAd,CAAkB,oBAAlB,CAAZ;EACD,CA1BY;;EA2BbC,OAAO,EAAC;IACNC,cAAc,CAACC,IAAD,EAAMC,OAAN,EAAc;MAC1B,KAAKC,OAAL,CAAaC,IAAb,CAAkB;QAChBC,IAAI,EAAE,eADU;QAEhBC,MAAM,EAAE;UACNL,IAAI,EAAEA;QADA;MAFQ,CAAlB;MAMAN,OAAO,CAACC,GAAR,CAAYM,OAAO,CAACK,EAApB;MAED,IAAIC,iBAAiB,GAAC,EAAtB;;MACA,IAAG,KAAKX,QAAL,CAAcC,GAAd,CAAkB,oBAAlB,MAA4C,IAA/C,EAAoD;QACjD;QACA,KAAKD,QAAL,CAAcC,GAAd,CAAkB,oBAAlB;MACH;;MACCU,iBAAiB,CAACJ,IAAlB,CAAuBF,OAAvB;MACA,IAAIO,QAAO,GAAGC,MAAM,CAACC,cAAP,CAAsBH,iBAAtB,EAAyCE,MAAM,CAACE,SAAhD,CAAd;MACA,KAAKf,QAAL,CAAcgB,GAAd,CAAkB,oBAAlB,EAAuCJ,QAAvC;IAED,CAnBK;;IAoBNK,qBAAqB,CAACb,IAAD,EAAMC,OAAN,EAAc;MACjC,KAAKC,OAAL,CAAaC,IAAb,CAAkB;QAChBC,IAAI,EAAE,uBADU;QAEhBC,MAAM,EAAE;UACNL,IAAI,EAAEA;QADA;MAFQ,CAAlB;MAMA,IAAIO,iBAAiB,GAAC,EAAtB;;MACD,IAAG,KAAKX,QAAL,CAAcC,GAAd,CAAkB,oBAAlB,MAA4C,IAA/C,EAAoD;QACjDU,iBAAgB,GAAI,KAAKX,QAAL,CAAcC,GAAd,CAAkB,oBAAlB,CAApB;MACF;;MACAH,OAAO,CAACC,GAAR,CAAYM,OAAO,CAACK,EAApB;MACAC,iBAAiB,CAACJ,IAAlB,CAAuBF,OAAvB;MACA,IAAIO,QAAO,GAAGC,MAAM,CAACC,cAAP,CAAsBH,iBAAtB,EAAyCE,MAAM,CAACE