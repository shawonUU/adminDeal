{"ast":null,"code":"import axios from 'axios';\nimport { ratingGenerator } from '@/HelpersFunction/Helpers';\nexport default {\n  props: ['slug'],\n\n  data() {\n    return {\n      auth: {\n        isAuthenticated: false,\n        user: {}\n      },\n      shopWiseProduct: [],\n      shopWiseProductHolder: [],\n      shopDetails: [],\n      ShowNotFound: 'Loading...',\n      currentPage: 1,\n      totalItems: \"\"\n    };\n  },\n\n  created() {\n    var user = localStorage.getItem(\"user\");\n\n    if (user !== null) {\n      user = JSON.parse(user);\n      this.auth.isAuthenticated = true;\n      this.auth.user = user;\n    }\n  },\n\n  mounted() {\n    // console.log(this.slug);\n    this.getShopWiseProduct(1);\n  },\n\n  methods: {\n    getShopWiseProduct(page) {\n      if (!this.shopWiseProductHolder[page]) {\n        this.ShowNotFound = 'Loading...';\n        axios.get(this.rootDomain + 'vue/v3/products/seller/' + this.slug + '?page=' + page).then(response => {\n          console.log(response.data[3]);\n          this.shopWiseProductHolder[page] = response.data[0];\n          this.shopWiseProduct = this.shopWiseProductHolder[page];\n          this.shopDetails = response.data[1];\n          this.totalItems = response.data[2];\n\n          if (response.data[0] < 1) {\n            this.ShowNotFound = 'Product Not Found!';\n          } else {\n            this.ShowNotFound = '';\n          }\n\n          this.scrollToTop();\n        }).catch(err => {\n          console.log(err);\n        });\n      } else {\n        this.shopWiseProduct = this.shopWiseProductHolder[page];\n        this.scrollToTop();\n      }\n    },\n\n    getRatings(rating, maxRating = 5) {\n      return ratingGenerator(rating, maxRating);\n    },\n\n    productDetails(slug) {\n      this.$router.push({\n        name: \"singleProduct\",\n        params: {\n          slug: slug\n        }\n      });\n    },\n\n    digitalProductDetails(slug) {\n      this.$router.push({\n        name: \"DigitalProductDetails\",\n        params: {\n          slug: slug\n        }\n      });\n    },\n\n    followShop(id) {\n      alert(this.auth.user.access_token);\n      return;\n\n      if (this.auth.isAuthenticated) {\n        axios.get(this.selfDomain + \"vueweb/followed_shop/\" + id, {\n          headers: {\n            Authorization: \"Bearer \" + this.auth.user.access_token\n          }\n        }).then(res => {\n          // console.log(res.data);\n          this.getFollowedShop();\n        }).catch(err => {});\n      }\n    },\n\n    scrollToTop() {\n      window.scrollTo(0, 400);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAoKA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAASC,eAAT,QAAgC,2BAAhC;AACA,eAAc;EACVC,KAAK,EAAC,CAAC,MAAD,CADI;;EAEVC,IAAI,GAAE;IACF,OAAM;MACFC,IAAI,EAAC;QACFC,eAAe,EAAE,KADf;QAEFC,IAAI,EAAE;MAFJ,CADH;MAKFC,eAAe,EAAC,EALd;MAMFC,qBAAqB,EAAC,EANpB;MAOFC,WAAW,EAAC,EAPV;MAQFC,YAAY,EAAC,YARX;MASFC,WAAW,EAAC,CATV;MAUFC,UAAU,EAAC;IAVT,CAAN;EAYH,CAfS;;EAgBVC,OAAO,GAAE;IACL,IAAIP,IAAG,GAAIQ,YAAY,CAACC,OAAb,CAAqB,MAArB,CAAX;;IACA,IAAGT,IAAG,KAAM,IAAZ,EAAiB;MACbA,IAAG,GAAIU,IAAI,CAACC,KAAL,CAAWX,IAAX,CAAP;MACA,KAAKF,IAAL,CAAUC,eAAV,GAA4B,IAA5B;MACA,KAAKD,IAAL,CAAUE,IAAV,GAAiBA,IAAjB;IACJ;EACH,CAvBS;;EAwBVY,OAAO,GAAE;IACL;IACA,KAAKC,kBAAL,CAAwB,CAAxB;EACH,CA3BS;;EA4BVC,OAAO,EAAC;IACJD,kBAAkB,CAACE,IAAD,EAAM;MACpB,IAAG,CAAC,KAAKb,qBAAL,CAA2Ba,IAA3B,CAAJ,EAAqC;QAClC,KAAKX,YAAL,GAAqB,YAArB;QACAV,KAAK,CAACsB,GAAN,CAAU,KAAKC,UAAL,GAAgB,yBAAhB,GAA0C,KAAKC,IAA/C,GAAoD,QAApD,GAA6DH,IAAvE,EACCI,IADD,CACOC,QAAD,IAAY;UACfC,OAAO,CAACC,GAAR,CAAYF,QAAQ,CAACvB,IAAT,CAAc,CAAd,CAAZ;UACG,KAAKK,qBAAL,CAA2Ba,IAA3B,IAAmCK,QAAQ,CAACvB,IAAT,CAAc,CAAd,CAAnC;UACA,KAAKI,eAAL,GAAuB,KAAKC,qBAAL,CAA2Ba,IAA3B,CAAvB;UACA,KAAKZ,WAAL,GAAmBiB,QAAQ,CAACvB,IAAT,CAAc,CAAd,CAAnB;UACA,KAAKS,UAAL,GAAkBc,QAAQ,CAACvB,IAAT,CAAc,CAAd,CAAlB;;UACA,IAAGuB,QAAQ,CAACvB,IAAT,CAAc,CAAd,IAAiB,CAApB,EAAsB;YACtB,KAAKO,YAAL,GAAqB,oBAArB;UACC,CAFD,MAEK;YACF,KAAKA,YAAL,GAAqB,EAArB;UACH;;UACA,KAAKmB,WAAL;QACL,CAbD,EAaGC,K