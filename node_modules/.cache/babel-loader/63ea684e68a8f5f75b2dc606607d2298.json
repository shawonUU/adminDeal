{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      auth: {\n        isAuthenticated: false,\n        user: {}\n      },\n      purchasePackages: \"\"\n    };\n  },\n\n  created() {\n    var user = localStorage.getItem(\"user\");\n\n    if (user !== null) {\n      user = JSON.parse(user);\n      this.auth.isAuthenticated = true;\n      this.auth.user = user;\n    }\n\n    this.getPurchasePackages(1);\n  },\n\n  mounted() {\n    this.emitter.emit(\"headerFooter\", false);\n    console.log('unmounted has been called');\n  },\n\n  unmounted() {\n    this.emitter.emit(\"headerFooter\", false);\n    console.log('unmounted has been called');\n  },\n\n  methods: {\n    getPurchasePackages(page) {\n      axios.get(this.selfDomain + 'vueseller_package/seller/packages-payment-list', {\n        headers: {\n          Authorization: \"Bearer \" + this.auth.user.access_token\n        }\n      }).then(response => {\n        this.purchasePackages = response.data.purchasePackages; //    this.offline_payment = response.data.offline_payment\n\n        console.log(response.data);\n      }).catch(err => {\n        console.log(err);\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAiDG,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;EACfC,IAAI,GAAE;IACF,OAAM;MACFC,IAAI,EAAC;QACDC,eAAe,EAAE,KADhB;QAEDC,IAAI,EAAE;MAFL,CADH;MAKEC,gBAAgB,EAAC;IALnB,CAAN;EAOH,CATc;;EAUfC,OAAO,GAAE;IACL,IAAIF,IAAG,GAAIG,YAAY,CAACC,OAAb,CAAqB,MAArB,CAAX;;IACI,IAAGJ,IAAG,KAAM,IAAZ,EAAiB;MACbA,IAAG,GAAIK,IAAI,CAACC,KAAL,CAAWN,IAAX,CAAP;MACA,KAAKF,IAAL,CAAUC,eAAV,GAA4B,IAA5B;MACA,KAAKD,IAAL,CAAUE,IAAV,GAAiBA,IAAjB;IACJ;;IACA,KAAKO,mBAAL,CAAyB,CAAzB;EACP,CAlBc;;EAmBfC,OAAO,GAAG;IACV,KAAKC,OAAL,CAAaC,IAAb,CAAkB,cAAlB,EAAkC,KAAlC;IACAC,OAAO,CAACC,GAAR,CAAY,2BAAZ;EACC,CAtBc;;EAuBfC,SAAS,GAAG;IACZ,KAAKJ,OAAL,CAAaC,IAAb,CAAkB,cAAlB,EAAkC,KAAlC;IACAC,OAAO,CAACC,GAAR,CAAY,2BAAZ;EACC,CA1Bc;;EA2BfE,OAAO,EAAC;IACJP,mBAAmB,CAACQ,IAAD,EAAM;MACrBnB,KAAK,CAACoB,GAAN,CAAU,KAAKC,UAAL,GAA