{"ast":null,"code":"import axios from 'axios';\nimport Select2 from 'vue3-select2-component';\nexport default {\n  components: {\n    Select2\n  },\n\n  data() {\n    return {\n      auth: {\n        isAuthenticated: false,\n        user: {}\n      },\n      categories: [],\n      brands: [],\n      refund_request: \"\",\n      pos_system: \"\",\n      product: {\n        productName: \"\",\n        unit: \"\",\n        weight: \"0.00\",\n        min_purchase_qty: \"1\",\n        barcode: \"\",\n        refundable: \"1\",\n        categoryId: \"\",\n        brandSlug: \"\",\n        tags: []\n      },\n      searchKey: '',\n      shipping_type: \"\"\n    };\n  },\n\n  created() {\n    var user = localStorage.getItem(\"user\");\n\n    if (user !== null) {\n      user = JSON.parse(user);\n      this.auth.isAuthenticated = true;\n      this.auth.user = user;\n    }\n\n    this.getCategories();\n  },\n\n  mounted() {\n    this.emitter.emit(\"headerFooter\", false);\n    console.log('unmounted has been called');\n  },\n\n  unmounted() {\n    this.emitter.emit(\"headerFooter\", false);\n    console.log('unmounted has been called');\n  },\n\n  methods: {\n    getCategories() {\n      axios.get(this.selfDomain + 'vueseller/seller/product/create', {\n        headers: {\n          Authorization: \"Bearer \" + this.auth.user.access_token\n        }\n      }).then(response => {\n        this.categories = response.data.categories;\n        this.brands = response.data.brands;\n        this.refund_request = response.data.refund_request;\n        this.pos_system = response.data.pos_system;\n        this.shipping_type = response.data.shipping_type;\n        console.log(response.data);\n      }).catch(err => {\n        console.log(err);\n      });\n    },\n\n    setCategory(category) {\n      this.product.categoryId = category;\n    },\n\n    setBrand(brand) {\n      this.product.brandSlug = brand.slug;\n    },\n\n    addTag(event) {\n      if (event.code == \"Comma\" || event.code == \"Enter\" || event.type == \"change\") {\n        event.preventDefault();\n        var val = event.target.value.trim();\n\n        if (val.length > 0) {\n          // if(this.tags.length <= 2){\n          //     this.tags.push(val)\n          //     event.target.value = ''\n          // }else{\n          //     event.target.value = ''\n          // }\n          this.product.tags.push(val);\n          event.target.value = '';\n          this.getAccessoriesList();\n        }\n      }\n    },\n\n    removeTag(index) {\n      this.product.tags.splice(index, 1);\n      this.getAccessoriesList();\n    },\n\n    removeLastTag(event) {\n      if (event.target.value.length === 0) {\n        this.removeTag(this.product.tags.length - 1);\n        this.getAccessoriesList();\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA4jBA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,OAAP,MAAoB,wBAApB;AACA,eAAe;EACXC,UAAU,EAAC;IAACD;EAAD,CADA;;EAEXE,IAAI,GAAE;IACF,OAAM;MACFC,IAAI,EAAC;QACDC,eAAe,EAAE,KADhB;QAEDC,IAAI,EAAE;MAFL,CADH;MAKFC,UAAU,EAAC,EALT;MAMFC,MAAM,EAAC,EANL;MAOFC,cAAc,EAAC,EAPb;MAQFC,UAAU,EAAC,EART;MASFC,OAAO,EAAC;QACJC,WAAW,EAAC,EADR;QAEJC,IAAI,EAAC,EAFD;QAGJC,MAAM,EAAC,MAHH;QAIJC,gBAAgB,EAAC,GAJb;QAKJC,OAAO,EAAC,EALJ;QAMJC,UAAU,EAAC,GANP;QAOJC,UAAU,EAAC,EAPP;QAQJC,SAAS,EAAC,EARN;QASJC,IAAI,EAAE;MATF,CATN;MAuBFC,SAAS,EAAC,EAvBR;MAwBFC,aAAa,EAAC;IAxBZ,CAAN;EA4BH,CA/BU;;EAgCXC,OAAO,GAAE;IACL,IAAIjB,IAAG,GAAIkB,YAAY,CAACC,OAAb,CAAqB,MAArB,CAAX;;IACI,IAAGnB,IAAG,KAAM,IAAZ,EAAiB;MACbA,IAAG,GAAIoB,IAAI,CAACC,KAAL,CAAWrB,IAAX,CAAP;MACA,KAAKF,IAAL,CAAUC,eAAV,GAA4B,IAA5B;MACA,KAAKD,IAAL,CAAUE,IAAV,GAAiBA,IAAjB;IACJ;;IACJ,KAAKsB,aAAL;EACH,CAxCU;;EAyCXC,OAAO,GAAG;IACV,KAAKC,OAAL,CAAaC,IAAb,CAAkB,cAAlB,EAAkC,KAAlC;IACAC,OAAO,CAACC,GAAR,CAAY,2BAAZ;EACC,CA5CU;;EA6CXC,SAAS,GAAG;IACZ,KAAKJ,OAAL,CAAaC,IAAb,CAAkB,cAAlB,EAAkC,KAAlC;IACAC,OAAO,CAACC,GAAR,CAAY,2BAAZ;EACC,CAhDU;;EAiDXE,OAAO,EAAC;IACJP,aAAa,GAAE;MACX5B,KAAK,CAACoC,GAAN,CAAU,KAAKC,UAAL,GAAgB,iCAA1B,EAA4D;QACxDC,OAAO,EAAE;UACLC,