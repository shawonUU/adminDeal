,['subcategories']),\n    // ...mapState(useCategoryStore,['featuredCategories']),\n  },\n\n  created() {\n    this.getSlider(this.rootDomain);\n    this.getCat(this.rootDomain);\n  },\n\n  mounted() {},\n\n  methods: {\n    // ...mapActions(useCategoryStore,['getCat']),\n    // ...mapActions(useCategoryStore,['getSubcategory']),\n    getSlider(rootDomain) {\n      axios.get(rootDomain + 'vue/v3/sliders').then(response => {\n        this.sliders = response.data.data;\n      });\n    },\n\n    getCat(rootDomain) {\n      axios.get(rootDomain).then(response => {\n        // console.log(response.data.featured_categories);\n        this.categories = response.data.categories;\n        this.featuredCategories = response.data.featured_categories.data;\n      });\n    },\n\n    getSubcategory(rootDomain, id) {\n      // this.categoryId = id;\n      if (typeof this.subcategoriesProxi[id] === 'undefined') {\n        axios.get(rootDomain + \"category/nav-element-list\", {\n          params: {\n            id: id\n          }\n        }).then(response => {\n          let subCategorys = response.data.subCategorys;\n          this.subcategoriesProxi[id] = subCategorys;\n          console.log(id);\n          this.subcategories = JSON.parse(JSON.stringify(this.subcategoriesProxi));\n          console.log(this.subcategories[id]);\n        });\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAoFA,OAAOA,WAAP,MAAwB,4BAAxB,C,CACA;;AACA,SAASC,MAAT,EAAiBC,WAAjB,QAAoC,YAApC;AACA,SAASC,QAAT,EAAmBC,UAAnB,EAA+BC,UAA/B,QAAiD,QAAjD,C,CACA;;AACA,OAAO,YAAP;AACA,OAAO,uBAAP;AACA,OAAO,uBAAP;AACA,OAAOC,WAAP,MAAwB,yDAAxB;AACA,OAAOC,UAAP,MAAuB,wDAAvB;AACA,OAAOC,gBAAP,MAA6B,8DAA7B;AACA,OAAOC,eAAP,MAA4B,6DAA5B;AACA,OAAOC,kBAAP,MAA+B,gEAA/B;AACA,OAAOC,qBAAP,MAAkC,mEAAlC;AACA,OAAOC,gBAAP,MAA6B,8DAA7B;AACA,OAAOC,cAAP,MAA2B,uBAA3B;AACA,OAAOC,gBAAP,MAA6B,wBAA7B;AAEA,OAAOC,aAAP,MAA0B,wDAA1B;AAEA,SAASC,gBAAT,QAAiC,oBAAjC;AACA,SAASC,QAAT,EAAmBC,UAAnB,QAAoC,OAApC;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;E