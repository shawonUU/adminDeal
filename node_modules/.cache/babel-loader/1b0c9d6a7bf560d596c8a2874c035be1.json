tegories = response.data.featured_categories.data;\r\n    })\r\n  },\r\n  getSubcategory(rootDomain,id){\r\n    // this.categoryId = id;\r\n    if(typeof this.subcategoriesProxi[id] === 'undefined'){\r\n      axios.get(rootDomain+\"category/nav-element-list\",{params:{id:id}})\r\n      .then((response)=>{\r\n          let subCategorys = response.data.subCategorys;\r\n          this.subcategoriesProxi[id] = subCategorys;\r\n          console.log(id);\r\n          this.subcategories = JSON.parse(JSON.stringify(this.subcategoriesProxi));\r\n          console.log(this.subcategories[id]);\r\n      })\r\n    }\r\n  }\r\n}\r\n\r\n}\r\n</script>\r\n\r\n<style>\r\n.swiper-button-next:after, .swiper-button-prev:after {\r\n    background: #2eb2ff00!important;\r\n    font-family: swiper-icons;\r\n    font-size: 20px!important;\r\n    text-transform: none!important;\r\n    letter-spacing: 0;\r\n    font-variant: initial;\r\n    line-height: 1;\r\n    color: #000!important;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}