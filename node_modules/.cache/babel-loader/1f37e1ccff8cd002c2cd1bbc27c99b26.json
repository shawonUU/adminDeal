{"ast":null,"code":"import { ref } from \"vue\";\nimport Slider from '@vueform/slider';\nimport axios from 'axios';\nexport default {\n  components: {\n    Slider\n  },\n\n  data() {\n    return {\n      blogs: [],\n      value: [0, 100],\n      currentPage: 1,\n      lastPage: \"\"\n    };\n  },\n\n  mounted() {\n    this.getBlogs(1);\n  },\n\n  methods: {\n    getBlogs(page) {\n      axios.get(this.rootDomain + 'vue/blog?page=' + page).then(response => {\n        this.blogs = response.data;\n        this.lastPage = response.data.meta.total;\n        this.scrollToTop();\n      }).catch(error => {});\n    },\n\n    getSlug(slug) {\n      this.$router.push({\n        name: \"BlogDetails\",\n        params: {\n          slug: slug\n        }\n      });\n    },\n\n    getRange(value) {\n      console.log($(\"#getRange\").html());\n    },\n\n    scrollToTop() {\n      window.scrollTo(0, 0);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAkCA,SAASA,GAAT,QAAoB,KAApB;AACA,OAAOC,MAAP,MAAmB,iBAAnB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,UAAU,EAAE;IACRF;EADQ,CADC;;EAIZG,IAAI,GAAE;IACL,OAAM;MACJC,KAAK,EAAC,EADF;MAEJC,KAAK,EAAE,CAAC,CAAD,EAAG,GAAH,CAFH;MAGJC,WAAW,EAAC,CAHR;MAIJC,QAAQ,EAAC;IAJL,CAAN;EAMA,CAXW;;EAYZC,OAAO,GAAE;IACN,KAAKC,QAAL,CAAc,CAAd;EACF,CAdW;;EAeZC,OAAO,EAAC;IACPD,QAAQ,CAACE,IAAD,EAAM;MACZV,KAAK,CAACW,GAAN,CAAU,KAAKC,UAAL,GAAgB,gBAAhB,GAAiCF,IAA3C,EACCG,IADD,CACOC,QAAD,IAAY;QACd,KAAKX,KAAL,GAAaW,QAAQ,CAACZ,IAAtB;QACA,KAAKI,QAAL,GAAgBQ,QAAQ,CAACZ,IAAT,CAAca,IAAd,CAAmBC,KAAnC;QACA,KAAKC,WAAL;MACH,CALD,EAMCC,KAND,CAMQC,KAAD,IAAS,CAEf,CARD;IASD,CAXM;;IAYPC,OAAO,CAACC,IAAD,EAAM;MACT,KAAKC,OAAL,CAAaC,IAAb,CAAkB;QAClBC,IAAI,EAAE,aADY;QAElBC,MAAM,EAAE;UACNJ,IAAI,EAAEA;QADA;MAFU,CAAlB;IAMH,CAnBM;;IAoBPK,QAAQ,CAACtB,KAAD,EAAO;MACZuB,OAAO,CAACC,GAAR,CAAYC,CAAC,CAAC,WAAD,CAAD,CAAeC,IAAf,EAAZ;IACF,CAtBM;;IAuBPb,WAAW,GAAG;MACdc,MAAM,CAACC,QAAP,CAAgB,CAAhB,EAAkB,CAAlB;IACF;;EAzBS;AAfI,CAAf","names":["ref","Slider","axios","components","data","blogs","value","currentPage","lastPage","mounted","getBlogs","methods","page","get","rootDomain","then","response","meta","total","scrollToTop","catch","error","getSlug","slug","$router","push","name","params","getRange","console","log","$","html","window","scrollTo"],"sourceRoot":"","sources":["E:\\adminDeal\\src\\components\\pages\\FrontEnd\\Blog.vue"],"sourcesContent":["<template>\r\n  <section class=\"pb-4\">\r\n  <div class=\"container\">\r\n    <div class=\"card-columns\">\r\n      <div v-for=\"(blog,index) in blogs.data\" :key=\"index\" class=\"card mb-3 overflow-hidden shadow-sm\">\r\n        <a style=\"cursor:pointer\" @click=\"getSlug(blog.slug)\" class=\"text-reset d-block\">\r\n          <img :src=\"blog.banner\"  :alt=\"blog.title\" class=\"img-fluid lazyload \">\r\n        </a>\r\n        <div class=\"p-4\">\r\n          <h2 class=\"fs-18 fw-600 mb-1\">\r\n            <a  style=\"cursor:pointer\" @click=\"getSlug(blog.slug)\" class=\"text-reset\">{{ blog.name }}</a>\r\n          </h2>\r\n          <div class=\"mb-2 opacity-50\">\r\n            <i>{{ blog.category }}</i>\r\n          </div>\r\n          <p class=\"opacity-70 mb-4\" v-html=\"blog.short_description\"></p>\r\n          <a  style=\"cursor:pointer\" @click=\"getSlug(blog.slug)\" class=\"btn btn-soft-primary\"> View More </a>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div class=\"aiz-pagination aiz-pagination-center mt-4\">\r\n          <vue-awesome-paginate\r\n          :total-items=\"lastPage\"\r\n          :items-per-page=\"12\"\r\n          :max-pages-shown=\"5\"\r\n          v-model=\"currentPage\"\r\n          @click=\"getBlogs\"\r\n        />\r\n    </div>\r\n  </div>\r\n</section>\r\n</template>\r\n\r\n<script>\r\nimport { ref } from \"vue\";\r\nimport Slider from '@vueform/slider';\r\nimport axios from 'axios';\r\nexport default {\r\n  components: {\r\n      Slider,\r\n    },\r\n   data(){\r\n    return{\r\n      blogs:[],\r\n      value: [0,100],\r\n      currentPage:1,\r\n      lastPage:\"\",\r\n    }\r\n   },\r\n   mounted(){\r\n      this.getBlogs(1);\r\n   },\r\n   methods:{\r\n    getBlogs(page){\r\n      axios.get(this.rootDomain+'vue/blog?page='+page)\r\n      .then((response)=>{\r\n          this.blogs = response.data;\r\n          this.lastPage = response.data.meta.total\r\n          this.scrollToTop();\r\n      })\r\n      .catch((error)=>{\r\n\r\n      });\r\n    },\r\n    getSlug(slug){\r\n        this.$router.push({\r\n        name: \"BlogDetails\",\r\n        params: {\r\n          slug: slug\r\n        }\r\n      });\r\n    },\r\n    getRange(value){\r\n       console.log($(\"#getRange\").html());\r\n    },\r\n    scrollToTop() {\r\n    window.scrollTo(0,0);\r\n  }\r\n   },\r\n   \r\n\r\n}\r\n\r\n</script>\r\n\r\n<style>\r\n  .pagination-container {\r\n    display: flex;\r\n    column-gap: 10px;\r\n  }\r\n  .paginate-buttons {\r\n    height: 40px;\r\n    width: 40px;\r\n    border-radius: 20px;\r\n    cursor: pointer;\r\n    background-color: rgb(194, 74, 27)\r\n    border: 1px solid rgb(217, 217, 217);\r\n    color: black;\r\n  }\r\n  </style>"]},"metadata":{},"sourceType":"module"}