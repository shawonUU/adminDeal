{"ast":null,"code":"import { ratingGenerator } from '@/HelpersFunction/Helpers';\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      productsCollection: [],\n      links: this.rootDomain + 'vue/products/allpost',\n      buttonIsLoading: true\n    };\n  },\n\n  created() {\n    this.loadMore(this.rootDomain);\n  },\n\n  mounted() {},\n\n  methods: {\n    loadMore() {\n      this.buttonIsLoading = true;\n      axios.get(this.links).then(response => {\n        this.productsCollection.push(response.data.data);\n        this.links = response.data.links.next;\n        this.buttonIsLoading = false;\n      });\n    },\n\n    productDetails(slug) {\n      this.$router.push({\n        name: \"singleProduct\",\n        params: {\n          slug: slug\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA0EA,SAASA,eAAT,QAAgC,2BAAhC;AACC,OAAOC,KAAP,MAAkB,OAAlB;AACD,eAAe;EACXC,IAAI,GAAE;IACF,OAAM;MACFC,kBAAkB,EAAE,EADlB;MAEFC,KAAK,EAAE,KAAKC,UAAL,GAAgB,sBAFrB;MAGFC,eAAe,EAAC;IAHd,CAAN;EAKH,CAPU;;EAOTC,OAAO,GAAE;IACP,KAAKC,QAAL,CAAc,KAAKH,UAAnB;EACH,CATU;;EAUXI,OAAO,GAAE,CAER,CAZU;;EAaXC,OAAO,EAAC;IACJF,QAAQ,GAAE;MACN,KAAKF,eAAL,GAAqB,IAArB;MACAL,KAAK,CAACU,GAAN,CAAU,KAAKP,KAAf,EACCQ,IADD,CACOC,QAAD,IAAY;QACd,KAAKV,kBAAL,CAAwBW,IAAxB,CAA6BD,QAAQ,CAACX,IAAT,CAAcA,IAA3C;QACA,KAAKE,KAAL,GAAaS,QAAQ,CAACX,IAAT,CAAcE,KAAd,CAAoBW,IAAjC;QACA,KAAKT,eAAL,GAAqB,KAArB;MACH,CALD;IAMH,CATG;;IAURU,cAAc,CAACC,IAAD,EAAM;MAClB,KAAKC,OAAL,CAAaJ,IAAb,CAAkB;QAChBK,IAAI,EAAE,eADU;QAEhBC,MAAM,EAAE;UACNH,IAAI,EAAEA;QADA;MAFQ,CAAlB;IAMF;;EAjBQ;AAbG,CAAf","names":["ratingGenerator","axios","data","productsCollection","links","rootDomain","buttonIsLoading","created","loadMore","mounted","methods","get","then","response","push","next","productDetails","slug","$router","name","params"],"sourceRoot":"","sources":["D:\\VUE\\admideal\\src\\components\\ResourceComponents\\ProductComponents\\AllProduct.vue"],"sourcesContent":["<template>\r\n  <div class=\"container 