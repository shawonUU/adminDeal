  recentViewProduct[product.id] = product;\n      this.$cookies.set('recentViewProducts', recentViewProduct);\n    },\n\n    removeWishlistItem(id, index) {\n      axios.get(this.selfDomain + 'vueweb/wishlists_remove', {\n        params: {\n          id: id\n        },\n        headers: {\n          Authorization: \"Bearer \" + this.auth.user.access_token\n        }\n      }).then(response => {\n        this.wishlist.splice(index, 1);\n        this.emitter.emit(\"reload\", true);\n      }).catch(err => {\n        console.log(err);\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAyEG,OAAOA,KAAP,MAAkB,OAAlB;AACC,eAAe;EAGXC,IAAI,GAAE;IACF,OAAM;MACFC,IAAI,EAAC;QACDC,eAAe,EAAE,KADhB;QAEDC,IAAI,EAAE;MAFL,CADH;MAKFC,QAAQ,EAAC,EALP;MAMFC,gBAAgB,EAAC,EANf;MAOFC,SAAS,EAAC;IAPR,CAAN;EASH,CAbU;;EAcXC,OAAO,GAAE;IACN,IAAIJ,IAAG,GAAIK,YAAY,CAACC,OAAb,CAAqB,MAArB,CAAX;;IACC,IAAGN,IAAG,KAAM,IAAZ,EAAiB;MACbA,IAAG,GAAIO,IAAI,CAACC,KAAL,CAAWR,IAAX,CAAP;MACA,KAAKF,IAAL,CAAUC,eAAV,GAA4B,IAA5B;MACA,KAAKD,IAAL,CAAUE,IAAV,GAAiBA,IAAjB;MACA,KAAKS,WAAL;IACJ;EACH,CAtBU;;EAuBXC,OAAO,EAAC;IAEJD,WAAW,GAAE;MACTE,OAAO,CAACC,GAAR,CAAY,8BAAZ,EADS,CAET;;MACAhB,KAAK,CAACiB,GAAN,CAAU,KAAKC,UAAL,GAAgB,kBAA1B,EAA8C;QAC1CC,OAAO,EAAE;UACLC,aAAa,EAAE,YAAY,KAAKlB,IAAL,CAAUE,IAAV,CAAeiB;QADrC;MADiC,CAA9C,EAIGC,IAJH,CAIQC,GAAG,IAAE;QACb,KAAKhB,SAAL,GAAiB,KAAjB;QACAQ,OAAO,CAACC,GAAR,CAAYO,GAAG,CAACtB,IAAhB;QACA,KAAKI,QAAL,GAAgBkB,GAAG,CAACtB,IAAJ,CAASI,QAAT,CAAkBJ,IAAlC;QACA,KAAKK,gBAAL,GAAwBiB,GAAG,CAACtB,IAAJ,CAASK,gBAAjC;QACIS,OAAO,CAACC,GAAR,CAAY,YAAZ;MACH,CAVD,EAUGQ,KAVH,CAUSC,GAAG,IAAE;QACVV,OAAO,CAACC,GAAR,CAAYS,GAAZ;MACH,CAZD;IAaH,CAlBG;;IAmBJC,cAAc,CAACC,IAAD,EAAMC,OAAN,EAAc;MACxB,KAAKC,OAAL,CAAaC,IAAb,CAAkB;QACdC,IAAI,EAAE,eADQ;QAEdC,MAAM,EAAE;UACRL,IAAI,EAAEA;QADE;MAFM,CAAlB;MAQA,IAAIM,iBAAiB,GAAC,EAAtB;;MACA,IAAG,KAAKC,QAAL,CAAcjB,GAAd,CAAkB,oBAAlB,MAA4C,IAA/C,EAAoD;QAChDgB,iBAAgB,GAAI,KAAKC,QAAL,CAAcjB,GAAd,CAAkB,oBAAlB,CAApB;MACJ;;MACAgB,iBAAiB,CAACL,OAAO,CAACO,EAAT,CAAjB,GAAgCP,OAAhC