{"ast":null,"code":"import axios from 'axios';\nimport { ratingGenerator } from '@/HelpersFunction/Helpers';\nexport default {\n  data() {\n    return {\n      featuredProducts: []\n    };\n  },\n\n  mounted() {\n    this.getTodaysDealProduct(this.rootDomain);\n  },\n\n  methods: {\n    getFeaturedProducts(rootDomain) {\n      axios.get(rootDomain + 'vue/v3/products/featured').then(response => {\n        this.featuredProducts = response.data.data;\n      });\n    },\n\n    getRatings(rating, maxRating = 5) {\n      return ratingGenerator(rating, maxRating);\n    },\n\n    productDetails(slug) {\n      this.$router.push({\n        name: \"singleProduct\",\n        params: {\n          slug: slug\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA4DE,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAASC,eAAT,QAAgC,2BAAhC;AACA,eAAe;EACXC,IAAI,GAAE;IACF,OAAM;MACHC,gBAAgB,EAAC;IADd,CAAN;EAGH,CALU;;EAMXC,OAAO,GAAE;IACL,KAAKC,oBAAL,CAA0B,KAAKC,UAA/B;EACH,CARU;;EASXC,OAAO,EAAC;IACNC,mBAAmB,CAACF,UAAD,EAAY;MAC3BN,KAAK,CAACS,GAAN,CAAUH,UAAU,GAAC,0BAArB,EACCI,IADD,CACOC,QAAD,IAAY;QACd,KAAKR,gBAAL,GAAwBQ,QAAQ,CAACT,IAAT,CAAcA,IAAtC;MACH,CAHD;IAIA,CANE;;IAOJU,UAAU,CAACC,MAAD,EAAQC,SAAS,GAAC,CAAlB,EAAoB;MAC3B,OAAOb,eAAe,CAACY,MAAD,EAAQC,SAAR,CAAtB;IACA,CATC;;IAUJC,cAAc,CAACC,IAAD,EAAM;MACd,KAAKC,OAAL,CAAaC,IAAb,CAAkB;QACdC,IAAI,EAAE,eADQ;QAEdC,MAAM,EAAE;UACRJ,IAAI,EAAEA;QADE;MAFM,CAAlB;IAMH;;EAjBC;AATG,CAAf","names":["axios","ratingGenerator","data","featuredProducts","mounted","getTodaysDealProduct","rootDomain","methods","getFeaturedProducts","get","then","response","getRatings","rating","maxRating","productDetails","slug","$router","push","name","params"],"sourceRoot":"","sources":["F:\\adminDeal\\src\\components\\ResourceComponents\\ProductComponents\\detailPages\\AllFeaturedProduct.vue"],"sourcesContent":["<template>\r\n    <div style=\"\">\r\n     <section class=\"mb-4\">\r\n        <div class=\"container\">\r\n           <div class=\" my-4 \">\r\n              <h1 class=\"h2 fw-600\">Featured Products</h1>\r\n           </div>\r\n           <div class=\"row gutters-5 row-cols-xxl-6 row-cols-xl-5 row-cols-lg-4 row-cols-md-3 row-cols-2\">\r\n              <div v-for=\"(product, index) in featuredProducts\" :key=\"index\" class=\"col mb-2\">\r\n                      <div class=\"aiz-card-box border border-light rounded hov-shadow-md mt-1 mb-2 has-transition bg-white\">\r\n                          <div class=\"position-relative\">\r\n                          <a  style=\"cursor:pointer\" @click=\"productDetails(product.slug)\" class=\"d-block\">\r\n                              <img class=\"img-fit lazyload mx-auto h-140px h-md-210px\" :src=\"product.thumbnail_image\" :data-src=\"product.thumbnail_image\" :alt=\"product.name\">\r\n                          </a>\r\n                          <div class=\"absolute-top-right aiz-p-hov-icon\">\r\n                              <a href=\"javascript:void(0)\"  data-toggle=\"tooltip\" data-title=\"Add to wishlist\" data-placement=\"left\">\r\n                              <i class=\"la la-heart-o\"></i>\r\n                              </a>\r\n                              <a href=\"javascript:void(0)\"  data-toggle=\"tooltip\" data-title=\"Add to compare\" data-placement=\"left\">\r\n                              <i class=\"las la-sync\"></i>\r\n                              </a>\r\n                              <a href=\"javascript:void(0)\"  data-toggle=\"tooltip\" data-title=\"Add to cart\" data-placement=\"left\">\r\n                              <i class=\"las la-shopping-cart\"></i>\r\n                              </a>\r\n                          </div>\r\n                          </div>\r\n                          <div class=\"p-md-3 p-2 text-left\">\r\n                          <div class=\"fs-15\"> \r\n                              <template v-if=\"product.base_discounted_price != product.main_price\">\r\n                                  <del class=\"fw-600 opacity-50 mr-1\">{{product.stroked_price}}</del>\r\n                                  <span class=\"text-primary fw-600\">{{product.main_price}}</span>\r\n                              </template>\r\n                              <template v-else>\r\n                                  <span class=\"fw-700 text-primary\">{{ product.main_price }}</span>\r\n                              </template>\r\n                             \r\n                              <span class=\"my-danger\" style=\"color: #000 !important; font-size: 12px;\">&nbsp;{{ product.discount }}</span>\r\n                          </div>\r\n                          <div class=\"rating rating-sm mt-1\">\r\n                                <template v-for=\"index in 5\" :key=\"index\">\r\n                                   <i v-if=\"index<=product.rating\" class = 'las la-star active'></i>\r\n                                   <i v-else class = 'las la-star'></i>\r\n                                </template>\r\n                                ({{ product.rating }})\r\n                              </div>\r\n                          <h3 class=\"fw-600 fs-13 text-truncate-2 lh-1-4 mb-0 h-35px\">\r\n                              <a style=\"cursor:pointer\" @click=\"productDetails(product.slug)\" class=\"d-block text-reset\">{{product.name}}</a>\r\n                          </h3> \r\n                              <!-- <div class=\"rounded px-2 mt-2 bg-soft-primary border-soft-primary border\"> Cashback:0 <span class=\"fw-700 float-right\">{{ product.earn_point }}</span>\r\n                              </div>  -->\r\n                          </div>\r\n                      </div>\r\n                  </div>\r\n           </div>\r\n        </div>\r\n     </section>\r\n  </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import axios from 'axios';\r\n  import { ratingGenerator } from '@/HelpersFunction/Helpers';\r\n  export default {\r\n      data(){\r\n          return{\r\n             featuredProducts:[]\r\n          }\r\n      },\r\n      mounted(){\r\n          this.getTodaysDealProduct(this.rootDomain);\r\n      },\r\n      methods:{\r\n        getFeaturedProducts(rootDomain){\r\n            axios.get(rootDomain+'vue/v3/products/featured')\r\n            .then((response)=>{\r\n                this.featuredProducts = response.data.data;\r\n            })\r\n           },\r\n          getRatings(rating,maxRating=5){\r\n             return ratingGenerator(rating,maxRating)\r\n            },\r\n          productDetails(slug){\r\n                this.$router.push({\r\n                    name: \"singleProduct\",\r\n                    params: {\r\n                    slug: slug\r\n                    }\r\n                });\r\n            },\r\n      }\r\n  }\r\n  </script>\r\n  \r\n  <style>\r\n  \r\n  </style>"]},"metadata":{},"sourceType":"module"}