{"ast":null,"code":"import { ref } from \"vue\";\nimport Slider from '@vueform/slider';\nimport axios from 'axios';\nexport default {\n  components: {\n    Slider\n  },\n\n  data() {\n    return {\n      blogs: [],\n      value: [0, 100],\n      currentPage: 1\n    };\n  },\n\n  mounted() {\n    this.getBlogs(1);\n  },\n\n  methods: {\n    getBlogs(page) {\n      axios.get(this.rootDomain + 'vue/blog?page=' + page).then(response => {\n        console.log(reasponse.data.meta.last_page);\n        this.blogs = response.data;\n        this.TotalPage = reasponse.data.meta.last_page;\n      }).catch(error => {});\n    },\n\n    getSlug(slug) {\n      this.$router.push({\n        name: \"BlogDetails\",\n        params: {\n          slug: slug\n        }\n      });\n    },\n\n    getRange(value) {\n      console.log($(\"#getRange\").html());\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAuCA,SAASA,GAAT,QAAoB,KAApB;AACA,OAAOC,MAAP,MAAmB,iBAAnB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,UAAU,EAAE;IACRF;EADQ,CADC;;EAIZG,IAAI,GAAE;IACL,OAAM;MACJC,KAAK,EAAC,EADF;MAEJC,KAAK,EAAE,CAAC,CAAD,EAAG,GAAH,CAFH;MAGJC,WAAW,EAAC;IAHR,CAAN;EAKA,CAVW;;EAWZC,OAAO,GAAE;IACN,KAAKC,QAAL,CAAc,CAAd;EACF,CAbW;;EAcZC,OAAO,EAAC;IACPD,QAAQ,CAACE,IAAD,EAAM;MACZT,KAAK,CAACU,GAAN,CAAU,KAAKC,UAAL,GAAgB,gBAAhB,GAAiCF,IAA3C,EACCG,IADD,CACOC,QAAD,IAAY;QACdC,OAAO,CAACC,GAAR,CAAYC,SAAS,CAACd,IAAV,CAAee,IAAf,CAAoBC,SAAhC;QACA,KAAKf,KAAL,GAAaU,QAAQ,CAACX,IAAtB;QACA,KAAKiB,SAAL,GAAiBH,SAAS,CAACd,IAAV,CAAee,IAAf,CAAoBC,SAArC;MACH,CALD,EAMCE,KAND,CAMQC,KAAD,IAAS,CAEf,CARD;IASD,CAXM;;IAYPC,OAAO,CAACC,IAAD,EAAM;MACT,KAAKC,OAAL,CAAaC,IAAb,CAAkB;QAClBC,IAAI,EAAE,aADY;QAElBC,MAAM,EAAE;UACNJ,IAAI,EAAEA;QADA;MAFU,CAAlB;IAMH,CAnBM;;IAoBPK,QAAQ,CAACxB,KAAD,EAAO;MACZU,OAAO,CAACC,GAAR,CAAYc,CAAC,CAAC,WAAD,CAAD,CAAeC,IAAf,EAAZ;IACF;;EAtBM;AAdI,CAAf","names":["ref","Slider","axios","components","data","blogs","value","currentPage","mounted","getBlogs","methods","page","get","rootDomain","then","response","console","log","