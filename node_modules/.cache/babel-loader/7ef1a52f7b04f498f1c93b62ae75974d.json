{"ast":null,"code":"import AddToCartModal from \"../../layouts/Modal/AddToCartModal.vue\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      products: [],\n      productId: '',\n      viewAddToCartModal: false\n    };\n  },\n\n  components: {\n    AddToCartModal\n  },\n\n  created() {\n    this.getCompareItem();\n  },\n\n  components: {},\n  methods: {\n    getCompareItem() {\n      var compareItem = localStorage.getItem(\"compare\");\n\n      if (compareItem != null) {\n        compareItem = JSON.parse(compareItem);\n      } else {\n        compareItem = [];\n      }\n\n      console.log(\"compareItem\");\n      axios.get(this.rootDomain + 'vueweb/compare', {\n        params: {\n          ids: compareItem\n        }\n      }).then(res => {\n        console.log(res.data.data);\n        this.products = res.data.data;\n      }).catch(err => {});\n    },\n\n    addTocartModal(productId) {\n      this.productId = productId;\n      this.viewAddToCartModal = true;\n      let ele = document.getElementsByTagName('body');\n      ele[0].classList.add(\"modal-open\");\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAyGI,OAAOA,cAAP,MAA2B,wCAA3B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;EACXC,IAAI,GAAE;IACF,OAAM;MAEFC,QAAQ,EAAC,EAFP;MAGFC,SAAS,EAAC,EAHR;MAIFC,kBAAkB,EAAE;IAJlB,CAAN;EAMH,CARU;;EASXC,UAAU,EAAE;IACRN;EADQ,CATD;;EAYXO,OAAO,GAAE;IACL,KAAKC,cAAL;EACH,CAdU;;EAeXF,UAAU,EAAE,EAfD;EAiBXG,OAAO,EAAE;IACND,cAAc,GAAE;MACX,IAAIE,WAAU,GAAIC,YAAY,CAACC,OAAb,CAAqB,SAArB,CAAlB;;MACA,IAAGF,WAAU,IAAK,IAAlB,EAAuB;QACnBA,WAAU,GAAIG,IAAI,CAACC,KAAL,CAAWJ,WAAX,CAAd;MAEH,CAHD,MAGK;QAACA,WAAU,GAAI,EAAd;MAAiB;;MAEvBK,OAAO,CAACC,GAAR,CAAY,aAAZ;MACAf,KAAK,CAACgB,GAAN,CAAU,KAAKC,UAAL,GAAgB,gBAA1B,EAA4C;QACxCC,MAAM,EAAE;UACLC,GAAG,EAAEV;QADA;MADgC,CAA5C,EAIGW,IAJH,CAIQC,GAAG,IAAE;QACVP,OAAO,CAACC,GAAR,CAAYM,GAAG,CAACpB,IAAJ,CAASA,IAArB;QACA,KAAKC,QAAL,GAAgBmB,GAAG,CAACpB,IAAJ,CAASA,IAAzB;MACF,CAPD,EAOGqB,KAPH,CAOSC,GAAG,IAAE,CAEb,CATD;IAUH,CAnBI;;IAoBLC,cAAc,CAACr