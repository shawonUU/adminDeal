     this.classified_product = response.data[5];\n        this.auction = response.data[6];\n        this.wallet_system = response.data[7];\n        this.club_point = response.data[8];\n        this.affiliate_system = response.data[9];\n        this.support_ticket = response.data[10];\n        console.log(response.data);\n      }).catch(error => {\n        console.error(error);\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAsRI,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,SAAP,MAAsB,iBAAtB;AACA,OAAOC,gBAAP,MAA6B,wBAA7B;AACA,OAAOC,YAAP,MAAyB,0BAAzB;AACA,OAAOC,eAAP,MAA4B,wBAA5B;AACA,OAAOC,QAAP,MAAqB,sBAArB;AACA,OAAOC,wBAAP,MAAqC,sCAArC;AACA,eAAe;EACXC,KAAK,EAAE,CAAC,eAAD,CADI;EAEXC,UAAU,EAAC;IAACP,SAAD;IAAWC,gBAAX;IAA4BC,YAA5B;IAAyCC,eAAzC;IAAyDC,QAAzD;IAAkEC;EAAlE,CAFA;;EAIXG,IAAI,GAAE;IACF,OAAM;MACFC,IAAI,EAAC;QACDC,eAAe,EAAE,KADhB;QAEDC,IAAI,EAAE;MAFL,CADH;MAKFC,iBAAiB,EAAC,EALhB;MAMFC,cAAc,EAAC,EANb;MAOFC,YAAY,EAAC,EAPX;MAQFC,eAAe,EAAC,EARd;MASFC,qBAAqB,EAAC,EATpB;MAUFC,cAAc,EAAC,EAVb;MAWFC,kBAAkB,EAAC,EAXjB;MAYFC,OAAO,EAAC,EAZN;MAaFC,aAAa,EAAC,KAbZ;MAcFC,iBAAiB,EAAC,KAdhB;MAeFC,aAAa,EAAC,EAfZ;MAgBFC,UAAU,EAAC,EAhBT;MAiBFC,gBAAgB,EAAC,EAjBf;MAkBFC,cAAc,EAAC;IAlBb,CAAN;EAqBH,CA1BU;;EA2BXC,OAAO,GAAE;IACL,IAAIf,IAAG,GAAIgB,YAAY,CAACC,OAAb,CAAqB,MAArB,CAAX;;IACA,IAAGjB,IAAG,KAAM,IAAZ,EAAiB;MACbA,IAAG,GAAIkB,IAAI,CAACC,KAAL,CAAWnB,IAAX,CAAP;MACA,KAAKF,IAAL,CAAUC,eAAV,GAA4B,IAA5B;MACA,KAAKD,IAAL,CAAUE,IAAV,GAAiBA,IAAjB;MACAoB,OAAO,CAACC,GAAR,CAAY,KAAKvB,IAAL,CAAUE,IAAtB;MACA,KAAKsB,YAAL,CAAkB,KAAKC,aAAvB;IACJ;EAEH,CArCU;;EAsCXC,OAAO,GAAE;IACL,KAAKC,iBAAL;EACH,CAxCU;;EAyCXC,OAAO,EAAC;IACJJ,YAAY,CAACK,IAAD,EAAO;MACf,KAAK1B,iBAAL,GAAyB0B,IAAzB;IACH,CAHG;;IAKJF,iBAAiB,GAAE;MACfrC,KAAK,CAACwC,GAAN,CAAU,KAAKC,UAAL,GAAgB,wBAA1B,EAAmD;QAC/CC,MAAM,EAAE;UACIC,KAAK,EAAE,KAAKjC,IAAL,CAAUE,IAAV,CAAegC;QAD1B;MADuC,CAAnD,EAKCC,IALD,CAKOC,QAAD,IAAc;QAChB,KAAKhC,cAAL,GAAsBgC,QAAQ,CAACrC,IAAT,CAAc,CAAd,CAAtB;QACA,KAAKM,YAAL,GAAoB+B,QAAQ,CAACrC,IAAT,CAAc,CAAd,CAApB;QACA,KAAKO,eAAL,GAA