{"ast":null,"code":"import axios from 'axios';\nimport { ratingGenerator } from '@/HelpersFunction/Helpers';\nexport default {\n  data() {\n    return {\n      bestDealProducts: []\n    };\n  },\n\n  mounted() {\n    this.getTodaysDealProduct(this.rootDomain);\n  },\n\n  methods: {\n    getTodaysDealProduct(rootDomain) {\n      axios.get(rootDomain + 'vue/v3/products/todays-deal').then(response => {\n        console.log(response.data.data);\n        this.bestDealProducts = response.data.data;\n      }).catch(err => {\n        console.log(err);\n      });\n    },\n\n    getRatings(rating, maxRating = 5) {\n      return ratingGenerator(rating, maxRating);\n    },\n\n    productDetails(slug) {\n      this.$router.push({\n        name: \"singleProduct\",\n        params: {\n          slug: slug\n        }\n      });\n    },\n\n    digitalProductDetails(slug) {\n      this.$router.push({\n        name: \"DigitalProductDetails\",\n        params: {\n          slug: slug\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA4DA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAASC,eAAT,QAAgC,2BAAhC;AACA,eAAe;EACXC,IAAI,GAAE;IACF,OAAM;MACFC,gBAAgB,EAAC;IADf,CAAN;EAGH,CALU;;EAMXC,OAAO,GAAE;IACL,KAAKC,oBAAL,CAA0B,KAAKC,UAA/B;EACH,CARU;;EASXC,OAAO,EAAC;IACRF,oBAAoB,CAACC,UAAD,EAAY;MAC5BN,KAAK,CAACQ,GAAN,CAAUF,UAAU,GAAC,6BAArB,EACCG,IADD,CACOC,QAAD,IAAY;QACdC,OAAO,CAACC,GAAR,CAAYF,QAAQ,CAACR,IAAT,CAAcA,IAA1B;QACA,KAAKC,gBAAL,GAAwBO,QAAQ,CAACR,IAAT,CAAcA,IAAtC;MACH,CAJD,EAKCW,KALD,CAKQC,GAAD,IAAO;QACVH,OAAO,CAACC,GAAR,CAAYE,GAAZ;MACH,CAPD;IAQC,CAVG;;IAWRC,UAAU,CAACC,MAAD,EAAQC,SAAS,GAAC,CAAlB,EAAoB;MAC9B,OAAOhB,eAAe,CAACe,MAAD,EAAQC,SAAR,CAAtB;IACC,CAbO;;IAcRC,cAAc,CAACC,IAAD,EAAM;MAChB,KAAKC,OAAL,CAAaC,IAAb,CAAkB;QAClBC,IAAI,EAAE,eADY;QAElBC,MAAM,EAAE;UACRJ,IAAI,EAAEA;QADE;MAFU,CAAlB;IAMH,CArBO;;IAsBRK,qBAAqB,CAACL,IAAD,EAAM;MACzB,KAAKC,OAAL,CAAaC,IAAb,CAAkB;QAChBC,IAAI,EAAE,uBADU;QAEhBC,MAAM,EAAE;UACNJ,IAAI,EAAEA;QADA;MAFQ,CAAlB;IAMD;;EA7BO;AATG,CAAf","names":["axios","ratingG