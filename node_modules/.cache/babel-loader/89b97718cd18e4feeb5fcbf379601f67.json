:void(0)\"  data-toggle=\"tooltip\" data-title=\"Add to cart\" data-placement=\"left\">\r\n                            <i class=\"las la-shopping-cart\"></i>\r\n                            </a>\r\n                        </div>\r\n                        </div>\r\n                        <div class=\"p-md-3 p-2 text-left\">\r\n                        <div class=\"fs-15\"> \r\n                            <template v-if=\"product.base_discounted_price != product.main_price\">\r\n                                <del class=\"fw-600 opacity-50 mr-1\">{{product.stroked_price}}</del>\r\n                                <span class=\"text-primary fw-600\">{{product.main_price}}</span>\r\n                            </template>\r\n                            <template v-else>\r\n                                <span class=\"fw-700 text-primary\">{{ product.main_price }}</span>\r\n                            </template>\r\n                           \r\n                            <span class=\"my-danger\" style=\"color: #000 !important; font-size: 12px;\">&nbsp;{{ product.discount }}</span>\r\n                        </div>\r\n                        <div class=\"rating rating-sm mt-1\">\r\n                              <template v-for=\"index in 5\" :key=\"index\">\r\n                                 <i v-if=\"index<=product.rating\" class = 'las la-star active'></i>\r\n                                 <i v-else class = 'las la-star'></i>\r\n                              </template>\r\n                              ({{ product.rating }})\r\n                            </div>\r\n                        <h3 class=\"fw-600 fs-13 text-truncate-2 lh-1-4 mb-0 h-35px\">\r\n                            <a style=\"cursor:pointer\" @click=\"productDetails(product.slug)\" class=\"d-block text-reset\">{{product.name}}</a>\r\n                        </h3> \r\n                            <!-- <div class=\"rounded px-2 mt-2 bg-soft-primary border-soft-primary border\"> Cashback:0 <span class=\"fw-700 float-right\">{{ product.earn_point }}</span>\r\n                            </div>  -->\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n         </div>\r\n      </div>\r\n   </section>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport { ratingGenerator } from '@/HelpersFunction/Helpers';\r\nexport default {\r\n    data(){\r\n        return{\r\n            bestDealProducts:[]\r\n        }\r\n    },\r\n    mounted(){\r\n        this.getTodaysDealProduct(this.rootDomain);\r\n    },\r\n    methods:{\r\n    getTodaysDealProduct(rootDomain){\r\n        axios.get(rootDomain+'vue/v3/products/todays-deal')\r\n        .then((response)=>{\r\n            console.log(response.data.data)\r\n            this.bestDealProducts = response.data.data;\r\n        })\r\n        .catch((err)=>{\r\n            console.log(err);\r\n        })\r\n        },\r\n    getRatings(rating,maxRating=5){\r\n    return ratingGenerator(rating,maxRating)\r\n    },\r\n    productDetails(slug){\r\n        this.$router.push({\r\n        name: \"singleProduct\",\r\n        params: {\r\n        slug: slug\r\n        }\r\n    });\r\n    },\r\n    digitalProductDetails(slug){\r\n      this.$router.push({\r\n        name: \"DigitalProductDetails\",\r\n        params: {\r\n          slug: slug\r\n        }\r\n      });\r\n    },\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module"}