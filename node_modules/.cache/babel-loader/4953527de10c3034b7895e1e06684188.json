{"ast":null,"code":"import { Swiper, SwiperSlide } from \"swiper/vue\";\nimport { ratingGenerator } from '@/HelpersFunction/Helpers'; // Import Swiper styles\n\nimport \"swiper/css\";\nimport \"swiper/css/free-mode\";\nimport \"swiper/css/pagination\";\nimport { FreeMode, Navigation } from \"swiper\";\nimport axios from \"axios\";\nexport default {\n  props: ['products'],\n\n  data() {\n    return {\n      modules: [FreeMode, Navigation]\n    };\n  },\n\n  components: {\n    Swiper,\n    SwiperSlide\n  },\n\n  mounted() {\n    console.log(this.products);\n  },\n\n  methods: {\n    productDetails(slug) {\n      this.$router.push({\n        name: \"singleProduct\",\n        params: {\n          slug: slug\n        }\n      });\n    },\n\n    digitalProductDetails(slug) {\n      this.$router.push({\n        name: \"DigitalProductDetails\",\n        params: {\n          slug: slug\n        }\n      });\n    },\n\n    getRatings(rating, maxRating = 5) {\n      return ratingGenerator(rating, maxRating);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAsEA,SAASA,MAAT,EAAiBC,WAAjB,QAAoC,YAApC;AACA,SAASC,eAAT,QAAgC,2BAAhC,C,CACA;;AACA,OAAO,YAAP;AACA,OAAO,sBAAP;AACA,OAAO,uBAAP;AACA,SAASC,QAAT,EAAkBC,UAAlB,QAAoC,QAApC;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;EACXC,KAAK,EAAC,CAAC,UAAD,CADK;;EAEXC,IAAI,GAAE;IACN,OAAM;MACJC,OAAO,EAAE,CAACL,QAAD,EAAUC,UAAV;IADL,CAAN;EAGD,CANY;;EAObK,UAAU,EAAE;IACVT,MADU;IAEVC;EAFU,CAPC;;EAWbS,OAAO,GAAE;IACPC,OAAO,CAACC,GAAR,CAAY,KAAKC,QAAjB;EACD,CAbY;;EAcbC,OAAO,EAAC;IACNC,cAAc,CAACC,IAAD,EAAM;MAClB,KAAKC,OAAL,CAAaC,IAAb,CAAkB;QAChBC,IAAI,EAAE,eADU;QAEhBC,MAAM,EAAE;UACNJ,IAAI,EAAEA;QADA;MAFQ,CAAlB;IAMD,CARK;;IASNK,qBAAqB,CAACL,IAAD,EAAM;MACzB,KAAKC,OAAL,CAAaC,IAAb,CAAkB;QAChBC,IAAI,EAAE,uBADU;QAEhBC,MAAM,EAAE;UACNJ,IAAI,EAAEA;QADA;MAFQ,CAAlB;IAMD,CAhBK;;IAiBNM,UAAU,CAACC,MAAD,EAAQC,SAAS,GAAC,CAAlB,EAAoB;MACvB,OAAOtB,eAAe,CAACqB,MAAD,EAAQC,SAAR,CAAtB;IACF;;EAnBC;AAdK,CAAf","names":["Swiper","SwiperSlide","ratingGenerator","FreeMode","Navigation","axios","props","data","modules","components","mounted","console","log","products","methods","productDetails","slug","$router","push","name","params","digitalProductDetails","getRatings","rating","maxRating"],"sourceRoot":"","sources":["E:\\adminDeal\\src\\components\\ResourceComponents\\ProductComponents\\product_box.vue"],"sourcesContent":["<template>\r\n   <swiper\r\n              :slidesPerView=\"2\"\r\n              :spaceBetween=\"10\"\r\n              :breakpoints=\"{\r\n                '640': {\r\n                  slidesPerView: 2,\r\n                  spaceBetween: 20,\r\n                },\r\n                '768': {\r\n                  slidesPerView: 4,\r\n                  spaceBetween: 40,\r\n                },\r\n                '1024': {\r\n                  slidesPerView: 5,\r\n                  spaceBetween: 50,\r\n                },\r\n              }\"\r\n               :navigation=\"true\"\r\n              :modules=\"modules\"\r\n              class=\"mySwiper\"\r\n            >\r\n        <swiper-slide v-for=\"(product, index) in products\" :key=\"index\">  \r\n            <div class=\"carousel-box\">\r\n           <div class=\"aiz-card-box  rounded hov-shadow-md mt-1 mb-2 has-transition bg-white\">\r\n             <span class=\"badge-custom\">OFF <span class=\"box ml-1 mr-0\">{{ product.discount }}</span>\r\n             </span>\r\n             <div class=\"position-relative\">\r\n               <a  style=\"cursor:pointer\" @click=\"product.digital==0?productDetails(product.slug):digitalProductDetails(product.slug)\">\r\n                 <img src=\"https://admindeal.com.bd/public/assets/img/placeholder.jpg\" :data-src=\"product.thumbnail_image\" class=\"img-fit lazyload mx-auto h-140px h-md-210px\"  alt=\"Black Plated Finger Ring For Mens\" >\r\n               </a>\r\n               <div class=\"absolute-top-right aiz-p-hov-icon\">\r\n                 <a href=\"javascript:void(0)\" onclick=\"addToWishList(5931)\" data-toggle=\"tooltip\" data-title=\"Add to wishlist\" data-placement=\"left\">\r\n                   <i class=\"la la-heart-o\"></i>\r\n                 </a>\r\n                 <a href=\"javascript:void(0)\" onclick=\"addToCompare(5931)\" data-toggle=\"tooltip\" data-title=\"Add to compare\" data-placement=\"left\">\r\n                   <i class=\"las la-sync\"></i>\r\n                 </a>\r\n                 <a href=\"javascript:void(0)\" onclick=\"showAddToCartModal(5931)\" data-toggle=\"tooltip\" data-title=\"Add to cart\" data-placement=\"left\">\r\n                   <i class=\"las la-shopping-cart\"></i>\r\n                 </a>\r\n               </div>\r\n             </div>\r\n             <div class=\"p-md-3 p-2 text-left\">\r\n               <div class=\"fs-15\">\r\n                 <del class=\"fw-600 opacity-50 mr-1\">{{ product.stroked_price }}</del>\r\n                 <span class=\"fw-700 text-primary\">{{ product.main_price }} <span class=\"my-danger\" style=\"color: #000 !important; font-size: 12px;\">&nbsp;{{ product.discount }}</span>\r\n                 </span>\r\n               </div>\r\n               <div class=\"rating rating-sm mt-1\">\r\n                      <template v-for=\"index in 5\" :key=\"index\">\r\n                          <i v-if=\"index<=product.rating\" class = 'las la-star active'></i>\r\n                          <i v-else class = 'las la-star'></i>\r\n                      </template>\r\n                      ({{ product.rating }})\r\n                </div>\r\n               <h3 class=\"fw-600 fs-13 text-truncate-2 lh-1-4 mb-0 h-35px\">\r\n                <a style=\"cursor:pointer\"  @click=\"product.digital==0?productDetails(product.slug):digitalProductDetails(product.slug)\"  :to=\"{name:'singleProduct'}\" class=\"d-block text-reset\">{{ product.name}}</a>\r\n               </h3>\r\n               <!-- <div class=\"rounded px-2 mt-2 bg-soft-primary border-soft-primary border\"> Cashback : <span class=\"fw-700 float-right\">0</span>\r\n               </div> -->\r\n             </div>\r\n           </div>\r\n         </div> \r\n        </swiper-slide>\r\n         \r\n      </swiper>\r\n</template>\r\n\r\n<script>\r\nimport { Swiper, SwiperSlide } from \"swiper/vue\";\r\nimport { ratingGenerator } from '@/HelpersFunction/Helpers';\r\n// Import Swiper styles\r\nimport \"swiper/css\";\r\nimport \"swiper/css/free-mode\";\r\nimport \"swiper/css/pagination\";\r\nimport { FreeMode,Navigation } from \"swiper\";\r\nimport axios from \"axios\";\r\nexport default {\r\n    props:['products'],\r\n    data(){\r\n    return{\r\n      modules: [FreeMode,Navigation],\r\n    }\r\n  },\r\n  components: {\r\n    Swiper,\r\n    SwiperSlide,\r\n  },\r\n  mounted(){\r\n    console.log(this.products);\r\n  },\r\n  methods:{\r\n    productDetails(slug){\r\n      this.$router.push({\r\n        name: \"singleProduct\",\r\n        params: {\r\n          slug: slug\r\n        }\r\n      });\r\n    },\r\n    digitalProductDetails(slug){\r\n      this.$router.push({\r\n        name: \"DigitalProductDetails\",\r\n        params: {\r\n          slug: slug\r\n        }\r\n      });\r\n    },\r\n    getRatings(rating,maxRating=5){\r\n           return ratingGenerator(rating,maxRating)\r\n        },\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module"}