{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      pageNumber: \"\",\n      auth: {\n        isAuthenticated: false,\n        user: {}\n      },\n      orders: [],\n      currentPage: 1,\n      totalItems: \"\",\n      paymentStatus: \"\",\n      delivery_status: \"\",\n      searchKey: ''\n    };\n  },\n\n  created() {\n    var user = localStorage.getItem(\"user\");\n\n    if (user !== null) {\n      user = JSON.parse(user);\n      this.auth.isAuthenticated = true;\n      this.auth.user = user;\n    }\n\n    this.getOrders(1);\n  },\n\n  mounted() {\n    this.emitter.emit(\"headerFooter\", false);\n    console.log('unmounted has been called');\n  },\n\n  unmounted() {\n    this.emitter.emit(\"headerFooter\", false);\n    console.log('unmounted has been called');\n  },\n\n  methods: {\n    getOrders(page) {\n      let payment_status = \"\";\n      let delivery_status = \"\";\n\n      if (this.paymentStatus != null) {\n        payment_status = this.paymentStatus;\n      }\n\n      if (this.delivery_status != null) {\n        delivery_status = this.delivery_status;\n      }\n\n      this.pageNumber = page;\n      axios.get(this.selfDomain + 'vueseller/seller/orders?page=' + page, {\n        headers: {\n          Authorization: \"Bearer \" + this.auth.user.access_token\n        },\n        params: {\n          payment_status: payment_status,\n          delivery_status: delivery_status\n        }\n      }).then(response => {\n        this.orders = response.data.orders;\n        this.totalItems = response.data.totalOrder;\n        console.log(response.data);\n      }).catch(err => {\n        console.log(err);\n      });\n    },\n\n    getFilterData() {\n      this.getOrders(1);\n    },\n\n    getOrderId(id) {\n      this.$router.push({\n        name: 'sellerOrderDetails',\n        params: {\n          id: id\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAiHA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;EACfC,I