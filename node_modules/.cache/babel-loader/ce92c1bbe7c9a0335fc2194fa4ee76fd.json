{"ast":null,"code":"import SubCategory from '../layouts/subCategory.vue'; // Import Swiper Vue.js components\n\nimport { Swiper, SwiperSlide } from \"swiper/vue\";\nimport { Autoplay, Pagination, Navigation } from \"swiper\"; // Import Swiper styles\n\nimport \"swiper/css\";\nimport \"swiper/css/pagination\";\nimport \"swiper/css/navigation\";\nimport TodaysOffer from \"../ResourceComponents/ProductComponents/TodaysOffer.vue\";\nimport NewProduct from \"../ResourceComponents/ProductComponents/NewProduct.vue\";\nimport FeaturedCategory from \"../ResourceComponents/ProductComponents/FeaturedCategory.vue\";\nimport AuctionProducts from \"../ResourceComponents/ProductComponents/AuctionProducts.vue\";\nimport BestSellingProduct from \"../ResourceComponents/ProductComponents/BestSellingProduct.vue\";\nimport HomeCategoriesProduct from \"../ResourceComponents/ProductComponents/HomeCategoriesProduct.vue\";\nimport FreelanceService from \"../ResourceComponents/ProductComponents/FreelanceService.vue\";\nimport BrandComponent from \"./BrandsComponent.vue\";\nimport SellersComponent from \"./SellersComponent.vue\";\nimport AllProductVue from '../ResourceComponents/ProductComponents/AllProduct.vue';\nimport { useCategoryStore } from \"@/Store/Categories\";\nimport { mapState, mapActions } from \"pinia\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      sliders: [],\n      modules: [Autoplay, Pagination, Navigation],\n      categories: [],\n      featuredCategories: []\n    };\n  },\n\n  components: {\n    TodaysOffer,\n    Swiper,\n    SwiperSlide,\n    SubCategory,\n    BrandComponent,\n    SellersComponent,\n    NewProduct,\n    FeaturedCategory,\n    BestSellingProduct,\n    AuctionProducts,\n    HomeCategoriesProduct,\n    FreelanceService,\n    AllProductVue\n  },\n  computed: {// ...mapState(useCategoryStore,['categories']),\n    // ...mapState(useCategoryStore,['subcategories']),\n    // ...mapState(useCategoryStore,['featuredCategories']),\n  },\n\n  created() {\n    this.getSlider(this.rootDomain);\n    this.getCat(this.rootDomain);\n  },\n\n  mounted() {},\n\n  methods: {\n    // ...mapActions(useCategoryStore,['getCat']),\n    // ...mapActions(useCategoryStore,['getSubcategory']),\n    getSlider(rootDomain) {\n      axios.get(rootDomain + 'vue/v3/sliders').then(response => {\n        this.sliders = response.data.data;\n      });\n    },\n\n    receiveCategorySlug(slug) {\n      this.$router.push({\n        name: 'CategoryWiseProduct',\n        params: {\n          slug: slug\n        }\n      });\n    },\n\n    getCat(rootDomain) {\n      axios.get(rootDomain).then(response => {\n        this.categories = response.data.categories;\n        this.featuredCategories = response.data.featured_categories.data;\n      });\n    },\n\n    getSubcategory(rootDomain, id, dx) {\n      let temp = JSON.parse(JSON.stringify(this.categories)); // alert(('subcategories' in temp[dx]));\n\n      if ('subcategories' in temp[dx] == false) {\n        axios.get(rootDomain + \"category/nav-element-list\", {\n          params: {\n            id: id\n          }\n        }).then(response => {\n          let subCategorys = response.data.subCategorys;\n          temp[dx]['subcategories'] = subCategorys;\n          this.categories = temp;\n        });\n      } else {\n        console.log('exist');\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAoFA,OAAOA,WAAP,MAAwB,4BAAxB,C,CACA;;AACA,SAASC,MAAT,EAAiBC,WAAjB,QAAoC,YAApC;AACA,SAASC,QAAT,EAAmBC,UAAnB,EAA+BC,UAA/B,QAAiD,QAAjD,C,CACA;;AACA,OAAO,YAAP;AACA,OAAO,uBAAP;AACA,OAAO,uBAAP;AACA,OAAOC,WAAP,MAAwB,yDAAxB;AACA,OAAOC,UAAP,MAAuB,wDAAvB;AACA,OAAOC,gBAAP,MAA6B,8DAA7B;AACA,OAAOC,eAAP,MAA4B,6DAA5B;AACA,OAAOC,kBAAP,MAA+B,gEAA/B;AACA,OAAOC,qBAAP,MAAkC,mEAAlC;AACA,OAAOC,gBAAP,MAA6B,8DAA7B;AACA,OAAOC,cAAP,MAA2B,uBAA3B;AACA,OAAOC,gBAAP,MAA6B,wBAA7B;AAEA,OAAOC,aAAP,MAA0B,wDAA1B;AAEA,SAASC,gBAAT,QAAiC,oBAAjC;AACA,SAASC,QAAT,EAAmBC,UAAnB,QAAoC,OAApC;AACA,OAAOC,KAAP,MAAkB,OAAlB