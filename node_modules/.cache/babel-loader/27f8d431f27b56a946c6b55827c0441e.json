{"ast":null,"code":"import AddressModal from \"../../layouts/Modal/addressModal.vue\";\nimport axios from 'axios';\nexport default {\n  components: {\n    AddressModal\n  },\n\n  data() {\n    return {\n      auth: {\n        isAuthenticated: false,\n        user: {}\n      },\n      addresses: [],\n      modalisOpen: false,\n      shippingInfo: true,\n      deliveryInfo: false,\n      adminId: '',\n      admin_products: [],\n      seller_products: [],\n      carts: [],\n      shipping_type: '',\n      site_name: '',\n      pickup_point_list: ''\n    };\n  },\n\n  created() {\n    var user = localStorage.getItem(\"user\");\n\n    if (user !== null) {\n      user = JSON.parse(user);\n      this.auth.isAuthenticated = true;\n      this.auth.user = user;\n      this.getShippingData();\n    }\n  },\n\n  computed() {},\n\n  mounted() {\n    this.emitter.on(\"addressModal\", message => {\n      this.modalisOpen = message;\n    });\n  },\n\n  methods: {\n    getShippingData() {\n      axios.get(this.selfDomain + \"vueweb/checkout\", {\n        headers: {\n          Authorization: \"Bearer \" + this.auth.user.access_token\n        }\n      }).then(res => {\n        console.log(res.data);\n        this.addresses = res.data.data;\n      }).catch(err => {});\n    },\n\n    addNewAddress() {\n      this.modalisOpen = true;\n    },\n\n    delivery_info() {\n      let address_id = document.querySelector('input[name=\"address_id\"]:checked').value;\n      this.shippingInfo = false;\n      this.deliveryInfo = true;\n      axios.get(this.selfDomain + \"vueweb/checkout/delivery_info\", {\n        params: {\n          address_id: address_id\n        },\n        headers: {\n          Authorization: \"Bearer \" + this.auth.user.access_token\n        }\n      }).then(res => {\n        console.log(res.data);\n        this.admin_products = res.data.admin_products;\n        this.seller_products = res.data.seller_products;\n        this.carrier_list = res.data.carrier_list;\n        this.carts = res.data.carts;\n        this.carts = res.data.carts;\n        this.carts = res.data.carts;\n        this.shipping_type = res.data.shipping_type;\n        this.site_name = res.data.site_name;\n        this.adminId = res.data.adminId;\n        this.pickup_point_list = res.data.pickup_point_list;\n      }).catch(err => {});\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAycE,OAAOA,YAAP,MAAyB,sCAAzB;AAEA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,UAAU,EAAC;IAACF;EAAD,CADE;;EAEbG,IAAI,GAAE;IACJ,OAAM;MACJC,IAAI,EAAC;QACHC,eAAe,EAAE,KADd;QAEHC,IAAI,EAAE;MAFH,CADD;MAKJC,SAAS,EAAE,EALP;MAMJC,WAAW,EAAE,KANT;MAOJC,YAAY,EAAE,IAPV;MAQJC,YAAY,EAAE,KARV;MAUJC,OAAO,EAAE,EAVL;MAWJC,cAAc,EAAE,EAXZ;MAYJC,eAAe,EAAE,EAZb;MAaJC,KAAK,EAAC,EAbF;MAcJC,aAAa,EAAE,EAdX;MAeJC,SAAS,EAAE,EAfP;MAgBJC,iBAAiB,EAAE;IAhBf,CAAN;EAkBD,CArBY;;EAsBbC,OAAO,GAAE;IACL,IAAIZ,IAAG,GAAIa,YAAY,CAACC,OAAb,CAAqB,MAArB,CAAX;;IACA,IAAGd,IAAG,KAAM,IAAZ,EAAiB;MACbA,IAAG,GAAIe,IAAI,CAACC,KAAL,CAAWhB,IAAX,CAAP;MACA,KAAKF,IAAL,CAAUC,eAAV,GAA4B,IAA5B;MACA,KAAKD,IAAL,CAAUE,IAAV,GAAiBA,IAAjB;MACA,KAAKiB,eAAL;IACJ;EACH,CA9BY;;EA+BbC,QAAQ,GAAE,CAET,CAjCY;;EAkCbC,OAAO,GAAE;IACL,KAAKC,OAAL,CAAaC,EAAb,CAAgB,cAAhB,EAAgCC,OAAM,IAAK;MACvC,KAAKpB,WAAL,GAAmBoB,OAAnB;IACH,CAFD;EAGH,CAtCY;;EAuCbC,OAAO,EAAC;IAEJN,eAAe,GAAE;MACbtB,KAAK,CAAC6B,GAAN,CAAU,KAAKC,UAAL,GAAgB,iBAA1B,EAA6C;QAC3CC,OAAO,EAAE;UACPC,aAAa,EAAE,YAAY,KAAK7B,IAAL,CAAUE,IAAV,CAAe4B;QADnC;MADkC,CAA7C,EAICC,IAJD,CAIMC,GAAG,IAAE;QACTC,OAAO,CAACC,GAAR,CAAYF,GAAG,CAACjC,IAAhB;QACA,KAAKI,SAAL,GAAiB6B,GAAG,CAACjC,IAAJ,CAASA,IAA1B;MACH,CAPC,EAOCoC,KAPD,CAOOC,GAAG,IAAE,CAEb,CATC;IAUH,CAbG;;IAcJC,aAAa,GAAE;MACX,KAAKjC,WAAL,GAAkB,IAAlB;IACH,CAhBG;;IAiBJkC,aAAa,GAAE;MACX,IAAIC,UAAS,GAAIC,QAAQ,CAACC,aAAT,CAAuB,kCAAvB,EAA2DC,KAA5E;MACA,KAAKrC,YAAL,GAAoB,KAApB;MACA,KAAKC,YAAL,GAAoB,IAApB;MAEIT,KAAK,CAAC6B,GAAN,CAAU,KAAKC,UAAL,GAAgB,+BAA1B,EAA2D;QACvDgB,MAAM,EAAC;UACHJ,UAAU,EAAEA;QADT,CADgD;QAIvDX,OAAO,EAAE;UACLC,aAAa,EAAE,YAAY,KAAK7B,IAAL,CAAUE,IAAV,CAAe4B;QADrC;MAJ8C,CAA3D,EAOGC